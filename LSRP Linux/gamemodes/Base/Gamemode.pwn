		{
			Kick(i);
			SendAdminAlert(COLOR_LIGHTRED, "[AdmWaring]: %s dang co gang dang nhap RCON tu IP \"%s\".", ReturnName(i), ip);
			Log_Write("logs/rcon_log.txt", "[%s] %s dang co gang dang nhan RCON tu IP \"%s\".", ReturnName(i), ReturnDate(), ip);
		}
		SendAdminAlert(COLOR_LIGHTRED, "[AdmWaring]: IP \"%s\" dang co gang dang nhap RCON.", ip);
		Log_Write("logs/rcon_log.txt", "[%s] IP \"%s\" dang co gang dang nhan RCON.", ReturnDate(), ip);
	}
	else
	{
		foreach(new i : Player) if(!strcmp(PlayerData[i][pIP], ip, true) && PlayerData[i][pAdmin] < 6)
		{
			Blacklist_Add(ip, PlayerData[i][pUsername], "Server", "Unauthorized RCON");

			SendAdminAlert(COLOR_LIGHTRED, "[AdmCmd]: %s da bi khoa tai khoan vi co gang dang nhap RCON.", ReturnName(i));
			Log_Write("logs/rcon_log.txt", "[%s] %s (%s) da bi khoa tai khoan vi co gang dang nhap RCON.", ReturnDate(), ReturnName(i), ip);

			break;
		}
	}
	return 1;
}

