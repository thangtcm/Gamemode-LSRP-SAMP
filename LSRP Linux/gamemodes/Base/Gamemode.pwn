CMD:editbillboard(playerid, params[])
{
	static
		id,
		type[24],
		string[128];

	if(PlayerData[playerid][pAdmin] < 5)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(sscanf(params, "ds[24]S()[128]", id, type, string))
 	{
		SendSyntaxMessage(playerid, "/editbillboard [id] [name]");
		SendClientMessage(playerid, COLOR_YELLOW, "Lua chon: location, name, price, message, owner, range.");
		return 1;
	}
	if((id < 0 || id >= MAX_BILLBOARDS) || !BillBoardData[id][bbExists])
		return SendErrorMessage(playerid, "ID billboard khong hop le.");

	if(!strcmp(type, "location", true))
	{
		GetPlayerPos(playerid, BillBoardData[id][bbPos][0], BillBoardData[id][bbPos][1], BillBoardData[id][bbPos][2]);

		Billboard_Refresh(id);
		Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi location billboard ID: %d.", ReturnName(playerid, 0), id);
	}
	else if(!strcmp(type, "price", true))
	{
		new price;

		if(sscanf(string, "d", price))
			return SendSyntaxMessage(playerid, "/editbillboard [id] [price] [new price]");

		BillBoardData[id][bbPrice] = price;

		Billboard_Refresh(id);
		Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi price billboard ID: %d thanh %s.", ReturnName(playerid, 0), id, FormatNumber(price));
	}
	else if(!strcmp(type, "name", true))
	{
		new name[32];

		if(sscanf(string, "s[32]", name))
			return SendSyntaxMessage(playerid, "/editbillboard [id] [name] [new name]");

		format(BillBoardData[id][bbName], 32, name);

		Billboard_Refresh(id);
		Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi ten billboard ID: %d thanh \"%s\".", ReturnName(playerid, 0), id, name);
	}
	else if(!strcmp(type, "message", true))
	{
		new name[32];

		if(sscanf(string, "s[230]", name))
			return SendSyntaxMessage(playerid, "/editbillboard [id] [message] [new message] (Ki tu toi da: 230)");

		format(BillBoardData[id][bbMessage], 32, name);

		Billboard_Refresh(id);
		Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi message billboard ID: %d thanh \"%s\".", ReturnName(playerid, 0), id, name);
	}
	else if(!strcmp(type, "owner", true))
	{
		new giveplayerid;

		if(sscanf(string, "d", giveplayerid))
		    return SendSyntaxMessage(playerid, "/editbillboard [id] [owner] [playerid]");

		if(giveplayerid == INVALID_PLAYER_ID)
			return SendErrorMessage(playerid, "Nguoi choi khong hop le.");

		BillBoardData[id][bbOwner] = GetPlayerSQLID(giveplayerid);

		Billboard_Refresh(id);
		Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi chu so huu billboard ID: %d", ReturnName(playerid, 0), id);
	}
	else if(!strcmp(type, "removeowner", true))
	{
		if(sscanf(string, "d"))
			return SendSyntaxMessage(playerid, "/editbillboard [id] [removeowner]");

		BillBoardData[id][bbOwner] = 0;

		Billboard_Refresh(id);
		Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da xoa chu so huu billboard ID: %d", ReturnName(playerid, 0), id);
	}
	else if(!strcmp(type, "range", true))
	{
		new range;

		if(sscanf(string, "d", range))
			return SendSyntaxMessage(playerid, "/editbillboard [id] [range] [new range]");

        if(range < 10)
		{
			SendErrorMessage(playerid, "Range phai tu 10-200");
			return 1;
		}

		if(range > 200)
		{
			SendErrorMessage(playerid, "Range phai tu 10-200");
			return 1;
		}

		BillBoardData[id][bbRange] = range;

		Billboard_Refresh(id);
		Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi range billboard ID: %d thanh %d.", ReturnName(playerid, 0), id, range);
	}
	return 1;
}

