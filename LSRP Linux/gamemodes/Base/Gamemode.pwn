{
	if(PlayerData[playerid][pAdmin] < 2)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	static
		userid,
		type[16],
		skinid[512];

	if(sscanf(params, "ud", userid, type, skinid))
	{
		SendSyntaxMessage(playerid, "/setclothes [playerid/name] [name] [skin ID]");
		SendClientMessage(playerid, COLOR_YELLOW, "Lua chon: clothes1, clothes2, clothes3.");
		return 1;
	}
	if(userid == INVALID_PLAYER_ID)
		return SendErrorMessage(playerid, "Nguoi choi khong hop le.");

	if(!strcmp(type, "clothes1", true))
	{
		if(isnull(skinid))
			return SendSyntaxMessage(playerid, "/setclothes [playerid/name] [name] [skin ID]");

		PlayerData[userid][pClothes1] = strval(skinid);
