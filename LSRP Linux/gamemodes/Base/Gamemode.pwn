			return SendErrorMessage(playerid, "Dien thoai nguoi do da tat nguon.");

		GiveMoney(playerid, -1);
		ShowPlayerFooter(playerid, "Ban da gui ~r~tin nhan~w~ va phi $1.");

		SendClientMessageEx(targetid, COLOR_YELLOW, "[SMS]: %s - %s (%d)", inputtext, ReturnName(playerid, 0), PlayerData[playerid][pPhone]);
		SendClientMessageEx(playerid, COLOR_YELLOW, "[SMS]: %s - %s (%d)", inputtext, ReturnName(playerid, 0), PlayerData[playerid][pPhone]);

		PlayerPlaySoundEx(targetid, 21001);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay dien thoai ra va gui tin nhan cho ai do.", ReturnName(playerid, 0));
	}
	else
	{
		Dialog_Show(playerid, SendText, DIALOG_STYLE_INPUT, "Gui tin nhan", "Xin vui long nhap so dien thoai ma ban muon gui tin toi:", "Xac nhan", "Quay lai");
