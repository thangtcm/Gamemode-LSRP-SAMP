	SendServerMessage(playerid, "Ban da hop dong vay tien cho %s. So luong: [%s]. Ly do: [%s]", ReturnName(userid, 0), FormatNumber(amount), reason);
	SendServerMessage(userid, "%s da hop dong vay tien cho ban, su dung [/chapnhan loan]. So luong: [%s]. Ly do: [%s]", ReturnName(playerid, 0), FormatNumber(amount), reason);

	Log_Write("logs/offer_loans.txt", "[%s] %s da hop dong vay tien cho %s. So luong: [%s]. Ly do: [%s] ", ReturnDate(), ReturnName(playerid, 0), ReturnName(userid, 0), FormatNumber(amount), reason);
	return 1;
}

