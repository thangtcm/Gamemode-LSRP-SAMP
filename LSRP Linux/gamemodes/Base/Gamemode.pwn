	if(PlayerData[playerid][pAdmin] < 1)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	new
		count,
		text[128];

	for(new i = 0; i != MAX_REPORTS; i++)
	{
		if(!ReportData[i][rExists])
			continue;

		strunpack(text, ReportData[i][rText]);

		SendClientMessageEx(playerid, COLOR_LIGHTYELLOW, "[Bao cao: %d] %s (ID: %d), ly do: %s", i, ReturnName(ReportData[i][rPlayer]), ReportData[i][rPlayer], text);
		count++;
	}
	if(!count)
		return SendErrorMessage(playerid, "Khong co ai bao cao.");

	SendServerMessage(playerid, "Su dung \"/ar\" hoac \"/dr\" de chap nhan hoac huy bo.");
	return 1;
}

