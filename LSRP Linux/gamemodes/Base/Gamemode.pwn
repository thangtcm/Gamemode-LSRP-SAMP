ResetDamages(playerid)
{
	DamageData[playerid][Misc] = 0;
	DamageData[playerid][Head] = 0;
	DamageData[playerid][Chest] = 0;
	DamageData[playerid][Crotch] = 0;
	DamageData[playerid][LArm] = 0;
	DamageData[playerid][RArm] = 0;
	DamageData[playerid][LLeg] = 0;
	DamageData[playerid][RLeg] = 0;
	for(new i = 0; i < 46; i++)
	{
		for(new z = 3; z <= 9; z++) // We start at 3 because 3 is the first body part id and the last one is 9, 1,2 and 10 are invalid.
		{
			if(Damage[playerid][i][Damages][z] > 0)
			{
				Damage[playerid][i][Damages][z] = 0;
			}
		}
	}
	return 1;
}

CountDamages(playerid)
{
	new count = 0;
	for(new i = 0; i < 46; i++)
	{
		for(new z = 3; z <= 9; z++) // We start at 3 because 3 is the first body part id and the last one is 9, 1,2 and 10 are invalid.
		{
			if(Damage[playerid][i][Damages][z] > 0)
			{
				count += Damage[playerid][i][Damages][z];
			}
		}
	}
	return count;
}

ReturnBodyPartName(bodypart)
{
	static
		part[20];

	switch(bodypart)
	{
		case BODY_PART_TORSO: part = "Toan Than";
		case BODY_PART_GROIN: part = "Hang";
		case BODY_PART_LEFT_ARM: part = "Tay Trai";
		case BODY_PART_RIGHT_ARM: part = "Tay Phai";
		case BODY_PART_LEFT_LEG: part = "Chan Trai";
		case BODY_PART_RIGHT_LEG: part = "Chan Phai";
		case BODY_PART_HEAD: part = "Dau";
		default: part = "Khong Co";
	}
	return part;
}

ShowPlayerDamages(toplayer, playerid)
{
	new
		name[32],
		damage[1024],
		gText[1000];

	format(name, sizeof(name), "%s", ReturnName(playerid));
	if(CountDamages(playerid) == 0)
	{
		return Dialog_Show(toplayer, DIALOG_DAMAGES, DIALOG_STYLE_LIST, name, "Khong co sat thuong nao tren nguoi.", "Dong lai", "");
	}
	for(new i = 0; i < 46; i++)
	{
		for(new z = 3; z <= 9; z++)
		{
			if(Damage[playerid][i][Damages][z] > 0)
			{
				switch(i)
				{
					case 0..15:
						format(damage, sizeof(damage), "%d cu dam %s sat thuong ngay %s\n", Damage[playerid][i][Damages][z], ReturnWeaponName(i), ReturnBodyPartName(z));
					case 54:
						format(damage, sizeof(damage), "%d roi xuong sat thuong ngay %s\n", Damage[playerid][i][Damages][z], ReturnBodyPartName(z));
					default:
						format(damage, sizeof(damage), "%d vien dan tu cay sung %s sat thuong ngay %s\n", Damage[playerid][i][Damages][z], ReturnWeaponName(i), ReturnBodyPartName(z));
				}
				strins(gText, damage, 0);
			}
		}
	}
	Dialog_Show(toplayer, DIALOG_DAMAGES, DIALOG_STYLE_LIST, name, gText, "Dong lai", "");
	return 1;
}

