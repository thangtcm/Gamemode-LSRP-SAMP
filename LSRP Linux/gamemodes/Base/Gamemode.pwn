		Entrance_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[AdmCmd]: %s da thay doi mapicon ID: %d thanh %d.", ReturnName(playerid, 0), id, icon);
	}
	else if(!strcmp(type, "password", true))
	{
		new password[32];

		if(sscanf(string, "s[32]", password))
			return SendSyntaxMessage(playerid, "/editdoor [id] [password] [entrance pass] (neu chinh 'none' se xoa mat khau)");

