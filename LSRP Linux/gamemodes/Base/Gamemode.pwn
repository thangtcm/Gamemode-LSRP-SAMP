CMD:spawnitem(playerid, params[])
{
	if(PlayerData[playerid][pAdmin] < 4)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(isnull(params))
		return SendSyntaxMessage(playerid, "/spawnitem [item name]");

	static
		Float:x,
		Float:y,
		Float:z;

	GetPlayerPos(playerid, x, y, z);

	for(new i = 0; i < sizeof(g_aInventoryItems); i++) if(!strcmp(g_aInventoryItems[i][e_InventoryItem], params, true))
	{
		new id = DropItem(g_aInventoryItems[i][e_InventoryItem], "Admin", g_aInventoryItems[i][e_InventoryModel], 1, x, y, z - 0.9, GetPlayerInterior(playerid), GetPlayerVirtualWorld(playerid));

		if(id == -1)
			return SendErrorMessage(playerid, "Ban khong the spawn them item.");
		Log_Write("logs/spawnitem.txt", "[%s] %s da spawn item %s", ReturnDate(), ReturnName(playerid, 0), g_aInventoryItems[i][e_InventoryItem]);
		SendServerMessage(playerid, "Ban da spawn item \"%s\" (su dung [/setquantity] de chinh so luong).", g_aInventoryItems[i][e_InventoryItem]);
		return 1;
	}
	SendErrorMessage(playerid, "Item khong hop le.");
	return 1;
}

CMD:setquantity(playerid, params[])
{
	static
		id = -1,
		amount;

	if(PlayerData[playerid][pAdmin] < 4)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if((id = Item_Nearest(playerid)) == -1)
		return SendErrorMessage(playerid, "Ban khong dung gan item.");

	if(sscanf(params, "d", amount))
		return SendSyntaxMessage(playerid, "/setquantity [amount]");

	if(amount < 1)
		return SendErrorMessage(playerid, "So luong khong the duoi 1.");

	Item_SetQuantity(id, amount);
	Log_Write("logs/spawnitem.txt", "[%s] %s da chinh so luong item %s toi %d.", ReturnDate(), ReturnName(playerid, 0), DroppedItems[id][droppedItem], amount);
	SendServerMessage(playerid, "Ban da chinh so luong item \"%s\" toi %d.", DroppedItems[id][droppedItem], amount);
	return 1;
}

