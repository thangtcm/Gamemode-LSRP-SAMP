public OnPlayerDeath(playerid, killerid, reason)
{
	if(killerid != INVALID_PLAYER_ID)
	{
		if(1 <= reason <= 46)
		{
			SendAdminAlert(COLOR_LIGHTRED, "{AA3333}[AdmWaring]{FFFF00}: %s da giet %s bang vu khi (%s).", ReturnName(killerid), ReturnName(playerid), ReturnWeaponName(reason));
			Log_Write("logs/kill_log.txt", "[%s] %s da giet %s bang vu khi (%s).", ReturnDate(), ReturnName(killerid), ReturnName(playerid), ReturnWeaponName(reason));
		}
		else
		{
			SendAdminAlert(COLOR_LIGHTRED, "{AA3333}[AdmWaring]{FFFF00}: %s da giet %s (ly do %d).", ReturnName(killerid), ReturnName(playerid), reason);
			Log_Write("logs/kill_log.txt", "[%s] %s da giet %s (ly do %d).", ReturnDate(), ReturnName(killerid), ReturnName(playerid), reason);
		}

		if(reason == 50 && killerid != INVALID_PLAYER_ID)
			SendAdminAlert(COLOR_LIGHTRED, "{AA3333}[AdmWaring]{FFFF00}: %s da giet %s boi Heli Blading.", ReturnName(killerid), ReturnName(playerid));

		if(reason == 29 && killerid != INVALID_PLAYER_ID && GetPlayerState(killerid) == PLAYER_STATE_DRIVER)
			SendAdminAlert(COLOR_LIGHTRED, "{AA3333}[AdmWaring]{FFFF00}: %s da giet %s boi Drive By.", ReturnName(killerid), ReturnName(playerid));
	}
	waittimer();
	return 1;
}

