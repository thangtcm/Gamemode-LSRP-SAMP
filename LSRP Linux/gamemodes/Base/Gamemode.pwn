forward Advertise(playerid);
public Advertise(playerid)
{
	if(!SQL_IsLogged(playerid) || !strlen(PlayerData[playerid][pAdvertise]))
		return 0;

	new
		text[128];

	strunpack(text, PlayerData[playerid][pAdvertise]);

	foreach(new i : Player) if(!PlayerData[i][pDisableBC])
	{
		if(strlen(text) > 64)
		{
			SendClientMessageEx(i, 0x00AA00FF, "[Quang cao]: %.64s", text);
			SendClientMessageEx(i, 0x00AA00FF, "[Quang cao]: ...%s [PH: %d]", text[64], PlayerData[playerid][pPhone]);
		}
		else
		{
			SendClientMessageEx(i, 0x00AA00FF, "[Quang cao]: %s (PH: %d)", text, PlayerData[playerid][pPhone]);
		}
	}
	PlayerData[playerid][pAdvertise][0] = 0;
	return 1;
}

public OnDialogResponse(playerid, dialogid, response, listitem, inputtext[])
{
	switch(dialogid)
	{
		case 12211:
		{
			if(!response)return 1;
			else
			{
				new string[128];
				format(string, sizeof(string), "[Helper]: %s dang can giup do, su dung [/chapnhantrogiup %d] de chap nhan giup do.", ReturnName(playerid, 0), playerid);
				SendHelperMessage(COLOR_NEWGREEN, string);
				format(string, sizeof(string), "[Cau hoi]: %s", inputtext);
				SendHelperMessage(COLOR_WHITE, string);
				AssistanceNeeded[playerid] = 1;
			}
		}
		case 12212:
		{
			if(!response)return 1;
			else
			{
				new string[128];
				format(string, sizeof(string), "[Faction]: %s dang yeu cau thay doi ten faction.", ReturnName(playerid, 0), playerid);
				SendAdminAlert(COLOR_NEWGREEN, string);
				format(string, sizeof(string), "[Chi tiet]: %s %s", inputtext), ReturnName(playerid, 0);
				SendAdminAlert(COLOR_WHITE, string);
			}
		}
		case 12250:
		{
			if(!response)return 1;
			else
			{
				switch(listitem)
				{
					case 0:
					{
						if(PlayerData[playerid][pMoney] >= 100)
						{
							PlayerData[playerid][pMoney] -= 100;
							new id = Inventory_Add(playerid, "Magazine", 2039);

							if(id == -1)
								return SendErrorMessage(playerid, "Ban khong con slot trong tui do.");

							SendClientMessage(playerid, COLOR_WHITE, "Ban da mua Magazine tai Black Market voi gia $100.");
						}
						else return SendErrorMessage(playerid, "Ban khong co du tien de mua.");
					}
					case 1:
					{
						if(PlayerData[playerid][pMoney] >= 200)
						{
							PlayerData[playerid][pMoney] -= 200;
							new id = Inventory_Add(playerid, "Ammo Cartridge", 2358);

							if(id == -1)
								return SendErrorMessage(playerid, "Ban khong con slot trong tui do.");

							SendClientMessage(playerid, COLOR_WHITE, "Ban da mua Ammo Cartridge tai Black Market voi gia $200.");
						}
						else return SendErrorMessage(playerid, "Ban khong co du tien de mua.");
					}
					case 2:
					{
						if(PlayerData[playerid][pMoney] >= 500)
						{
							if(Inventory_Count(playerid, "Armored Vest") >= 3)
								return SendErrorMessage(playerid, "Ban da co 3 Armored Vest, ban khong the mua tiep.");
								
							new id = Inventory_Add(playerid, "Armored Vest", 19142);

							if(id == -1)
								return SendErrorMessage(playerid, "Ban khong con slot trong tui do.");

							PlayerData[playerid][pMoney] -= 500;
							SendClientMessage(playerid, COLOR_WHITE, "Ban da mua Armored Vest tai Black Market voi gia $500.");
						}
						else return SendErrorMessage(playerid, "Ban khong co du tien de mua.");
					}
					case 3:
					{
						if(PlayerData[playerid][pMoney] >= 1000)
						{
							PlayerData[playerid][pMoney] -= 1000;
							SetPlayerArmour(playerid, 100.0);
							SendClientMessage(playerid, COLOR_WHITE, "Ban da mua Heavy Armored Vest tai Black Market voi gia $1000.");
						}
						else return SendErrorMessage(playerid, "Ban khong co du tien de mua.");
					}
					case 4:
					{
						if(PlayerData[playerid][pMoney] >= 500)
						{
							PlayerData[playerid][pMoney] -= 500;
							new id = Inventory_Add(playerid, "Bomb", 1654);

							if(id == -1)
								return SendErrorMessage(playerid, "Ban khong con slot trong tui do.");

							SendClientMessage(playerid, COLOR_WHITE, "Ban da mua Boom tai Black Market voi gia $500.");
						}
						else return SendErrorMessage(playerid, "Ban khong co du tien de mua.");
					}
				}
			}
		}
	}
	return 0;
}

