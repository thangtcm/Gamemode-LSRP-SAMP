{
	new
		defuseMessage[128],
		pName[MAX_PLAYER_NAME];

	GetPlayerName(explosiveData[idOfExplosive][explosiveOwner], pName, sizeof(pName));

	if(explosiveData[idOfExplosive][explosiveOwner] != INVALID_PLAYER_ID)
