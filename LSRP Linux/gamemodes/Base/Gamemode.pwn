		return SendErrorMessage(playerid, "Hay doi vai giay ban moi co the hut ma tuy tiep.");

	if(!strcmp(params, "marijuana", true))
	{
		if(!Inventory_HasItem(playerid, "Bat lua"))
			return SendErrorMessage(playerid, "Ban can bat lua, hay mua no tai shop 24/7.");

