CMD:createhouse(playerid, params[])
{
	static
		price,
		id,
		address[32];

	if(PlayerData[playerid][pAdmin] < 5)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(sscanf(params, "ds[32]", price, address))
		return SendSyntaxMessage(playerid, "/createhouse [price] [address]");

	for(new i = 0; i != MAX_HOUSES; i++) if(HouseData[i][houseExists] && !strcmp(HouseData[i][houseAddress], address, true))
	{
		return SendErrorMessage(playerid, "Ten dia chi \"%s\" da duoc su dung (ID: %d).", address, i);
	}
	id = House_Create(playerid, address, price);

	if(id == -1)
		return SendErrorMessage(playerid, "Ban khong the tao them house.");

	SendServerMessage(playerid, "Ban da tao house ID: %d.", id);
	return 1;
}

CMD:destroyhouse(playerid, params[])
{
	static
	    id = 0;

	if(PlayerData[playerid][pAdmin] < 5)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(sscanf(params, "d", id))
		return SendSyntaxMessage(playerid, "/destroyhouse [house id]");

	if((id < 0 || id >= MAX_HOUSES) || !HouseData[id][houseExists])
		return SendErrorMessage(playerid, "ID house khong hop le.");

	House_Delete(id);
	SendServerMessage(playerid, "Ban da xoa house ID: %d.", id);
	return 1;
}

