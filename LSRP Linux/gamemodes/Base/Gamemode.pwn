			{
				SendClientMessageEx(i, 0xAAC4E5FF, "[OOC] %s: %.64s", ReturnName(playerid), params);
				SendClientMessageEx(i, 0xAAC4E5FF, "[OOC] ...%s", params[64]);
			}
		}
	}
	else
	{
		foreach(new i : Player) if(!PlayerData[i][pDisableOOC] && PlayerData[i][pCreated])
		{
			if(strcmp(PlayerData[playerid][pForumName], "Null"))
			{
				SendClientMessageEx(i, 0xAAC4E5FF, "[OOC] %s (%s): %s", ReturnName(playerid, 0), PlayerData[playerid][pForumName], params);
