			return SendErrorMessage(playerid, "Ban khong the su dung ma tuy vi ban dang co day giap.");

        PlayerData[playerid][pDrugTime] = 35;
		PlayerData[playerid][pDrugUsed] = 2;

		Inventory_Remove(playerid, "Cocaine", 2);

		ApplyAnimation(playerid, "SMOKING", "M_smk_in", 4.1, 0, 0, 0, 0, 0, 1);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da su dung ma tuy Cocaine.", ReturnName(playerid, 0));
		GivePlayerArmour(playerid, 20);
		SendServerMessage(playerid, "Viec su dung ma tuy Cocaine ban duoc them +20 Armour.");
	}
	else if(!strcmp(params, "heroin", true))
	{
		if(!Inventory_HasItem(playerid, "Bat lua"))
			return SendErrorMessage(playerid, "Ban can bat lua, hay mua no tai shop 24/7.");

		if(Inventory_Count(playerid, "Heroin") < 2)
			return SendErrorMessage(playerid, "Ban can 2 gram ma tuy heroin.");

		PlayerData[playerid][pDrugTime] = 30;
		PlayerData[playerid][pDrugUsed] = 3;

		Inventory_Remove(playerid, "Heroin", 2);

		ApplyAnimation(playerid, "SMOKING", "M_smk_in", 4.1, 0, 0, 0, 0, 0, 1);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da su dung ma tuy Heroin.", ReturnName(playerid, 0));
	}
	else if(!strcmp(params, "steroids", true))
	{
	    if(!Inventory_HasItem(playerid, "Steroids"))
	        return SendErrorMessage(playerid, "Ban khong co ma tuy Steroids.");

		if(ReturnHealth(playerid) <= 5)
		    return SendErrorMessage(playerid, "Mau cua ban qua thap.");

        PlayerData[playerid][pDrugTime] = 60;
		PlayerData[playerid][pDrugUsed] = 4;

		SetPlayerHealth(playerid, ReturnHealth(playerid) - 5);
		Inventory_Remove(playerid, "Steroids", 2);

		ApplyAnimation(playerid, "SMOKING", "M_smk_in", 4.1, 0, 0, 0, 0, 0, 1);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da su dung ma tuy Steroids.", ReturnName(playerid, 0));
	}
	return 1;
}

