CMD:blacklist(playerid, params[])
{
	static
		type[24];

	if(PlayerData[playerid][pAdmin] < 3)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(sscanf(params, "s[24]s[128]", type, params))
	{
		SendSyntaxMessage(playerid, "/blacklist [name] [parameter]");
		SendClientMessage(playerid, COLOR_YELLOW, "Lua chon: ban, banip, unbanip.");
		return 1;
	}
	if(!strcmp(type, "ban", true))
	{
		if(!IsValidPlayerName(params))
			return SendErrorMessage(playerid, "Nguoi choi khong hop le.");

		foreach(new i : Player) if(!strcmp(PlayerData[i][pUsername], params) || !strcmp(ReturnName(i), params, true))
		{
			KickEx(i);
		}
		Blacklist_Add("0.0.0.0", params, PlayerData[playerid][pUsername], "Tai khoan bi khoa [/blacklist].");

		SendServerMessage(playerid, "Ban da khoa tai khoan \"%s\".", params);
		SendAdminAlert(COLOR_LIGHTRED, "[AdmCmd]: %s da khoa tai khoan \"%s\".", ReturnName(playerid), params);

		Log_Write("logs/ban_log.txt", "[%s] %s da khoa tai khoan \"%s\".", ReturnDate(), ReturnName(playerid), params);
	}
	else if(!strcmp(type, "banip", true))
	{
		if(!IsAnIP(params))
			return SendErrorMessage(playerid, "Dia chi IP khong hop le.");

		foreach(new i : Player) if(!strcmp(PlayerData[i][pIP], params))
		{
			KickEx(i);
		}
		Blacklist_Add(params, "", PlayerData[playerid][pUsername], "IP da bi khoa [/blacklist].");

		SendServerMessage(playerid, "Ban da khoa IP \"%s\".", params);
		SendAdminAlert(COLOR_LIGHTRED, "[AdmCmd]: %s da khoa IP \"%s\".", ReturnName(playerid), params);

		Log_Write("logs/ban_log.txt", "[%s] %s da khoa IP \"%s\".", ReturnDate(), ReturnName(playerid), params);
	}
	else if(!strcmp(type, "unbanip", true))
	{
		if(!IsAnIP(params))
			return SendErrorMessage(playerid, "Dia chi IP khong hop le.");

		Blacklist_RemoveIP(params);

		SendServerMessage(playerid, "Ban da mo khoa IP \"%s\".", params);
		SendAdminAlert(COLOR_LIGHTRED, "[AdmCmd]: %s da mo khoa IP \"%s\".", ReturnName(playerid), params);

		Log_Write("logs/ban_log.txt", "[%s] %s da mo khoa IP \"%s\".", ReturnDate(), ReturnName(playerid), params);
	}
	return 1;
}

