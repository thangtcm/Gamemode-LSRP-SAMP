	Inventory_Remove(playerid, "Binh xang");
	GameTextForPlayer(playerid, "~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~~r~Dang do xang...", 5200, 3);

	SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay binh xang va do xang vao phuong tien.", ReturnName(playerid, 0));
	SetTimerEx("RefillUpdate", 5000, false, "dd", playerid, vehicleid);
	return 1;
}

