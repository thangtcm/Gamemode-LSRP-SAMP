CMD:kickdoor(playerid, params[])
{
	static
		id = -1;

	if(GetFactionType(playerid) != FACTION_POLICE)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if((id = House_Nearest(playerid)) != -1)
	{
		if(!HouseData[id][houseLocked])
			return SendErrorMessage(playerid, "Can nha nay khong bi khoa.");

		ShowPlayerFooter(playerid, "Tien hanh ~r~dap~w~ cua...");
		ApplyAnimation(playerid, "POLICE", "Door_Kick", 4.0, 0, 0, 0, 0, 0);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s dang tien hanh dap cua.", ReturnName(playerid, 0));
		SetTimerEx("KickHouse", 1500, false, "dd", playerid, id);
	}
	else if((id = Business_Nearest(playerid)) != -1)
	{
		if(!BusinessData[id][bizLocked])
			return SendErrorMessage(playerid, "Cua hang nay khong bi khoa.");

		ShowPlayerFooter(playerid, "Tien hanh ~r~dap~w~ cua...");
		ApplyAnimation(playerid, "POLICE", "Door_Kick", 4.0, 0, 0, 0, 0, 0);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s dang tien hanh dap cua.", ReturnName(playerid, 0));
		SetTimerEx("KickBusiness", 1500, false, "dd", playerid, id);
	}
	else
	{
		SendErrorMessage(playerid, "Ban khong dung gan can nha hoac cua hang.");
	}
	return 1;
}

CMD:buyrack(playerid, params[])
{
	new houseid = House_Inside(playerid);

	if(houseid == -1 || !House_IsOwner(playerid, houseid))
		return SendErrorMessage(playerid, "Ban khong ben trong can nha.");

	if(isnull(params) || (!isnull(params) && strcmp(params, "confirm", true) != 0))
		return SendSyntaxMessage(playerid, "/buyrack [confirm] ($1,000)");

	if(Rack_Count(playerid) >= 4)
		return SendErrorMessage(playerid, "Ban co the co 4 tu dung vu khi.");

	if(Rack_Nearest(playerid) != -1)
		return SendErrorMessage(playerid, "ban khong the mua tu dung vu khi gan nhung tu dung khac.");

	if(GetMoney(playerid) < 1000)
		return SendErrorMessage(playerid, "Ban khong du tien de mua.");

	new id = Rack_Create(playerid, houseid);

	if(id == -1)
		return SendErrorMessage(playerid, "Ban khong the mua them tu dung vu khi.");

	ResetEditing(playerid);

	PlayerData[playerid][pEditRack] = id;
	EditDynamicObject(playerid, RackData[id][rackObjects][4]);

	GiveMoney(playerid, -1000);
	SendServerMessage(playerid, "Ban da mua tu dung vu khi voi gia $1,000.");
	return 1;
}

CMD:gunrack(playerid, params[])
{
	new id = Rack_Nearest(playerid);

