		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(GetVehicleModel(GetPlayerVehicleID(playerid)) != 525)
		return SendErrorMessage(playerid, "Ban khong co tren phuong tien TowTruck.");

	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER)
		return SendErrorMessage(playerid, "Ban khong phai la nguoi lai xe.");

	new
		trailerid = GetVehicleTrailer(GetPlayerVehicleID(playerid));

	if(!trailerid)
		return SendErrorMessage(playerid, "Khong co phuong tien nao gan day.");

	DetachTrailerFromVehicle(GetPlayerVehicleID(playerid));
	SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da ha can truc va ngung keo chiec %s.", ReturnName(playerid, 0), ReturnVehicleName(trailerid));

	return 1;
}

CMD:giamxe(playerid, params[])
{
	new
		price,
		id = Impound_Nearest(playerid),
		vehicleid = GetPlayerVehicleID(playerid);

	if(GetFactionType(playerid) != FACTION_POLICE)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(sscanf(params, "d", price))
		return SendSyntaxMessage(playerid, "/giamxe [price]");

	if(price < 1 || price > 1000)
		return SendErrorMessage(playerid, "So tien khong the duoi $1 hoac cao hon $1,000.");

	if(GetVehicleModel(vehicleid) != 525)
		return SendErrorMessage(playerid, "Ban khong co tren phuong tien TowTruck.");

	if(id == -1)
		return SendErrorMessage(playerid, "Ban khong dung gan cho giam xe.");

	if(!GetVehicleTrailer(vehicleid))
		return SendErrorMessage(playerid, "Khong co phuong tien nao duoc keo.");

	vehicleid = Car_GetID(GetVehicleTrailer(vehicleid));

	if(vehicleid == -1)
		return SendErrorMessage(playerid, "Ban khong the keo phuong tien nay.");

	if(CarData[vehicleid][carImpounded] != -1)
		return SendErrorMessage(playerid, "Phuong tien nay da duoc giam giu.");

	CarData[vehicleid][carImpounded] = ImpoundData[id][impoundID];
	CarData[vehicleid][carImpoundPrice] = price;

	Tax_AddMoney(price);

	GetVehiclePos(CarData[vehicleid][carVehicle], CarData[vehicleid][carPos][0], CarData[vehicleid][carPos][1], CarData[vehicleid][carPos][2]);
	Car_Save(vehicleid);

	SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "Radio: %s da giam phuong tien %s voi so luong tien %s.", ReturnName(playerid, 0), ReturnVehicleModelName(CarData[vehicleid][carModel]), FormatNumber(price));
	DetachTrailerFromVehicle(GetPlayerVehicleID(playerid));
	return 1;
}

