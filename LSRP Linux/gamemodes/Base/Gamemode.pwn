		GetPlayerPos(playerid, x, y, z);
		GetPlayerFacingAngle(playerid, angle);

		x += 3.0 * floatsin(-angle, degrees);
		y += 3.0 * floatcos(-angle, degrees);

		GateData[id][gatePos][0] = x;
		GateData[id][gatePos][1] = y;
		GateData[id][gatePos][2] = z;
		GateData[id][gatePos][3] = 0.0;
		GateData[id][gatePos][4] = 0.0;
		GateData[id][gatePos][5] = angle;

		SetDynamicObjectPos(GateData[id][gateObject], x, y, z);
		SetDynamicObjectRot(GateData[id][gateObject], 0.0, 0.0, angle);

		GateData[id][gateOpened] = false;

		Gate_Save(id);
		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi location gate ID: %d.", ReturnName(playerid, 0), id);
		return 1;
	}
	else if(!strcmp(type, "speed", true))
	{
		static
			Float:speed;

		if(sscanf(string, "f", speed))
			return SendSyntaxMessage(playerid, "/editgate [id] [speed] [move speed]");

		if(speed < 0.0 || speed > 20.0)
			return SendErrorMessage(playerid, "Speed khong the duoi 0 hoac cao hon 20.");

        GateData[id][gateSpeed] = speed;

		Gate_Save(id);
		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi speed gate ID: %d thanh %.2f.", ReturnName(playerid, 0), id, speed);
		return 1;
	}
	else if(!strcmp(type, "radius", true))
	{
		static
			Float:radius;

		if(sscanf(string, "f", radius))
			return SendSyntaxMessage(playerid, "/editgate [id] [radius] [open radius]");

		if(radius < 0.0 || radius > 20.0)
			return SendErrorMessage(playerid, "Radius khong the duoi 0 hoac cao hon 20.");

		GateData[id][gateRadius] = radius;

		Gate_Save(id);
		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi radius gate ID: %d thanh %.2f.", ReturnName(playerid, 0), id, radius);
		return 1;
	}
	else if(!strcmp(type, "time", true))
	{
		static
			time;

		if(sscanf(string, "d", time))
			return SendSyntaxMessage(playerid, "/editgate [id] [time] [close time] (0 de vo hieu hoa)");

		if(time < 0 || time > 60000)
			return SendErrorMessage(playerid, "Time khong the duoi 0 hoac cao hon 60,000 ms.");

		GateData[id][gateTime] = time;

		Gate_Save(id);
		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay toi time gate ID: %d thanh %d.", ReturnName(playerid, 0), id, time);
		return 1;
	}
	else if (!strcmp(type, "model", true))
	{
		static
			model;

		if(sscanf(string, "d", model))
			return SendSyntaxMessage(playerid, "/editgate [id] [model] [gate model]");

		if(!IsValidObjectModel(model))
			return SendErrorMessage(playerid, "Object model khong hop le.");

		GateData[id][gateModel] = model;

		DestroyDynamicObject(GateData[id][gateObject]);
		GateData[id][gateObject] = CreateDynamicObject(GateData[id][gateModel], GateData[id][gatePos][0], GateData[id][gatePos][1], GateData[id][gatePos][2], GateData[id][gatePos][3], GateData[id][gatePos][4], GateData[id][gatePos][5], GateData[id][gateWorld], GateData[id][gateInterior]);

		Gate_Save(id);
		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi object model gate ID: %d thanh %d.", ReturnName(playerid, 0), id, model);
		return 1;
	}
    else if(!strcmp(type, "pos", true))
	{
		ResetEditing(playerid);
		EditDynamicObject(playerid, GateData[id][gateObject]);

		PlayerData[playerid][pEditGate] = id;
		PlayerData[playerid][pEditType] = 1;

		SendServerMessage(playerid, "Ban da thay doi pos gate ID: %d.", id);
		return 1;
	}
	else if(!strcmp(type, "move", true))
	{
		ResetEditing(playerid);
		EditDynamicObject(playerid, GateData[id][gateObject]);

		PlayerData[playerid][pEditGate] = id;
		PlayerData[playerid][pEditType] = 2;

		SendServerMessage(playerid, "Ban da thay doi move gate ID: %d.", id);
		return 1;
	}
	else if(!strcmp(type, "linkid", true))
	{
		static
			linkid = -1;

		if(sscanf(string, "d", linkid))
			return SendSyntaxMessage(playerid, "/editgate [id] [linkid] [gate link] (-1 de vo hieu hoa)");

		if((linkid < -1 || linkid >= MAX_GATES) || (linkid != -1 && !GateData[linkid][gateExists]))
			return SendErrorMessage(playerid, "ID gate khong hop le.");

		GateData[id][gateLinkID] = (linkid == -1) ? (-1) : (GateData[linkid][gateID]);
		Gate_Save(id);

		if(id == -1)
			SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da vo hieu hoa faction gate ID: %d.", ReturnName(playerid, 0), id);

		else
			SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi faction gate ID: %d thanh ID: %d.", ReturnName(playerid, 0), id, linkid);

		return 1;
	}
	else if(!strcmp(type, "faction", true))
	{
		static
			factionid = -1;

		if(sscanf(string, "d", factionid))
			return SendSyntaxMessage(playerid, "/editgate [id] [faction] [gate faction] (-1 de vo hieu hoa)");

		if((factionid < -1 || factionid >= MAX_FACTIONS) || (factionid != -1 && !FactionData[factionid][factionExists]))
			return SendErrorMessage(playerid, "Faction khong hop le.");

		GateData[id][gateFaction] = (factionid == -1) ? (-1) : (FactionData[factionid][factionID]);
		Gate_Save(id);

		if(factionid == -1)
			SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da vo hieu hoa faction gate ID: %d.", ReturnName(playerid, 0), id);

		else
			SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi faction gate ID: %d thanh \"%s\".", ReturnName(playerid, 0), id, FactionData[factionid][factionName]);

		return 1;
	}
	else if(!strcmp(type, "pass", true))
	{
		static
			pass[32];

		if(sscanf(string, "s[32]", pass))
			return SendSyntaxMessage(playerid, "/editgate [id] [pass] [gate password] ('none' de vo hieu hoa)");

		if(!strcmp(params, "none", true))
			GateData[id][gatePass][0] = 0;

		else format(GateData[id][gatePass], 32, pass);

		Gate_Save(id);
		SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da thay doi password gate ID: %d thanh %s.", ReturnName(playerid, 0), id, pass);
		return 1;
	}
	return 1;
}

