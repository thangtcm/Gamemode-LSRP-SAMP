stock SetPlayerToFacePlayer(playerid, targetid)
{
	new
		Float:x[2],
		Float:y[2],
		Float:z[2],
		Float:angle;

	GetPlayerPos(targetid, x[0], y[0], z[0]);
	GetPlayerPos(playerid, x[1], y[1], z[1]);

	angle = (180.0 - atan2(x[1] - x[0], y[1] - y[0]));
	SetPlayerFacingAngle(playerid, angle + (5.0 * -1));
}

