CMD:spike(playerid, params[])
{
	if(GetFactionType(playerid) != FACTION_POLICE)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(isnull(params))
	{
		SendSyntaxMessage(playerid, "/spike [option]");
		SendClientMessage(playerid, COLOR_YELLOW, "Lua chon: drop, destroy, destroyall.");
		return 1;
	}
	static
		Float:fX,
		Float:fY,
		Float:fZ,
		Float:fA;

	GetPlayerPos(playerid, fX, fY, fZ);
	GetPlayerFacingAngle(playerid, fA);

	if(!strcmp(params, "drop", true))
	{
		if(IsPlayerInAnyVehicle(playerid))
			return SendErrorMessage(playerid, "Ban phai ra khoi phuong tien.");

		for(new i = 0; i != MAX_BARRICADES; i++) if(!BarricadeData[i][cadeExists])
		{
			BarricadeData[i][cadeExists] = true;
			BarricadeData[i][cadeType] = 1;

			BarricadeData[i][cadePos][0] = fX;
			BarricadeData[i][cadePos][1] = fY;
			BarricadeData[i][cadePos][2] = fZ;

			BarricadeData[i][cadeObject] = CreateDynamicObject(2899, fX, fY, fZ - 0.8, 0.0, 0.0, fA + 90.0);

			SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da dai dinh sat.", ReturnName(playerid, 0));
			SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "Radio: %s da dai dinh sat tai %s.", ReturnName(playerid, 0), GetLocation(fX, fY, fZ));

			return 1;
		}
		SendErrorMessage(playerid, "Ban khong the dat them dinh sat.");
	}
	else if(!strcmp(params, "destroy", true))
	{
		for(new i = 0; i != MAX_BARRICADES; i++) if(BarricadeData[i][cadeExists] && BarricadeData[i][cadeType] == 1 && IsPlayerInRangeOfPoint(playerid, 3.0, BarricadeData[i][cadePos][0], BarricadeData[i][cadePos][1], BarricadeData[i][cadePos][2]))
		{
			BarricadeData[i][cadeExists] = 0;
			BarricadeData[i][cadeType] = 0;

			DestroyDynamicObject(BarricadeData[i][cadeObject]);

			SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da pha huy dinh sat.", ReturnName(playerid, 0));
			SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "Radio: %s da pha huy dinh sat tai %s.", ReturnName(playerid, 0), GetLocation(fX, fY, fZ));
			return 1;
		}
		SendErrorMessage(playerid, "Ban khong dung gan nhung dinh sat.");
	}
	else if(!strcmp(params, "destroyall", true))
	{
		for(new i = 0; i != MAX_BARRICADES; i++) if(BarricadeData[i][cadeExists] && BarricadeData[i][cadeType] == 1)
		{
			BarricadeData[i][cadeExists] = 0;
			BarricadeData[i][cadeType] = 0;

			DestroyDynamicObject(BarricadeData[i][cadeObject]);
		}
		SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "Radio: %s da pha huy tat ca dinh sat.", ReturnName(playerid, 0));
	}
	return 1;
}

