	for(new i = 0; i != MAX_FACTIONS; i++) if(FactionData[i][factionExists])
	{
		SendClientMessageEx(playerid, COLOR_WHITE, "[Faction: %d] {%06x}%s", i, FactionData[i][factionColor] >>> 8, FactionData[i][factionName]);
	}

	if(sscanf(params, "ud", userid, id))
		return SendSyntaxMessage(playerid, "/setleader [playerid/name] [faction id] (Use -1 to unset)");

	if(userid == INVALID_PLAYER_ID)
		return SendErrorMessage(playerid, "Nguoi choi khong hop le.");

	if((id < -1 || id >= MAX_FACTIONS) || (id != -1 && !FactionData[id][factionExists]))
		return SendErrorMessage(playerid, "Faction khong hop le.");

	if(id == -1)
	{
		ResetFaction(userid);
		PlayerData[userid][pLeader] = -1;

		SendServerMessage(playerid, "Ban da duoi %s ra khoi faction leader.", ReturnName(userid, 0));
		SendServerMessage(userid, "%s da duoi ban ra khoi faction leader.", ReturnName(playerid, 0));
	}
	else
	{
		SetFaction(userid, id);
		PlayerData[userid][pFactionRank] = FactionData[id][factionRanks];
		PlayerData[userid][pFactionDivision] = 1;
		PlayerData[userid][pLeader] = 1;

		new badgenumber = random(99999) + 1000;
		if(GetFactionType(playerid) == FACTION_POLICE || GetFactionType(playerid) == FACTION_MEDIC || GetFactionType(playerid) == FACTION_GOV)
		{
			PlayerData[playerid][pBadgeNumber] = badgenumber;
			SendServerMessage(playerid, "So huy hieu cua ban la %d.", badgenumber);
		}

		SendServerMessage(playerid, "Ban da cho %s lam leader \"%s\".", ReturnName(userid, 0), FactionData[id][factionName]);
		SendServerMessage(userid, "%s da cho ban lam leader \"%s\".", ReturnName(playerid, 0), FactionData[id][factionName]);
	}
	return 1;
}

