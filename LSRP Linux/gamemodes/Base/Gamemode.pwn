CMD:buy(playerid, params[])
{
	static
		id = -1;

	if((id = House_Nearest(playerid)) != -1)
	{
		if(House_GetCount(playerid) >= MAX_OWNABLE_HOUSES)
			return SendErrorMessage(playerid, "Ban co the so huu %d house.", MAX_OWNABLE_HOUSES);

		if(HouseData[id][houseOwner] != 0)
			return SendErrorMessage(playerid, "House nay da co chu so huu.");

		if(HouseData[id][housePrice] > GetMoney(playerid))
			return SendErrorMessage(playerid, "Ban khong co du tien de mua.");

		HouseData[id][houseOwner] = GetPlayerSQLID(playerid);
		strcat((HouseData[id][houseOwnerName][0] = 0, HouseData[id][houseOwnerName]), ReturnName(playerid, 0), MAX_PLAYER_NAME);

		House_Refresh(id);
		House_Save(id);

		GiveMoney(playerid, -HouseData[id][housePrice]);
		SendServerMessage(playerid, "Ban da mua house \"%s\" voi gia %s.", HouseData[id][houseAddress], FormatNumber(HouseData[id][housePrice]));

		ShowPlayerFooter(playerid, "Ban da ~g~mua~w~ house.");
		Log_Write("logs/house_log.txt", "[%s] %s da mua house ID: %d voi gia %s.", ReturnDate(), ReturnName(playerid), id, FormatNumber(HouseData[id][housePrice]));
	}
	else if((id = Business_Nearest(playerid)) != -1)
	{
		if(Business_GetCount(playerid) >= MAX_OWNABLE_BUSINESSES)
			return SendErrorMessage(playerid, "Ban co the so huu %d business.", MAX_OWNABLE_BUSINESSES);

		if(BusinessData[id][bizOwner] != 0)
			return SendErrorMessage(playerid, "Business da co chu so huu.");

		if(BusinessData[id][bizPrice] > GetMoney(playerid))
			return SendErrorMessage(playerid, "Ban khong co du tien de mua.");

	    BusinessData[id][bizOwner] = GetPlayerSQLID(playerid);
	    strcat((BusinessData[id][bizOwnerName][0] = 0, BusinessData[id][bizOwnerName]), ReturnName(playerid, 0), MAX_PLAYER_NAME);

		Business_Refresh(id);
		Business_Save(id);

	    GiveMoney(playerid, -BusinessData[id][bizPrice]);
	    SendServerMessage(playerid, "Ban da mua business \"%s\" voi gia %s.", BusinessData[id][bizName], FormatNumber(BusinessData[id][bizPrice]));

		ShowPlayerFooter(playerid, "Ban da ~g~mua~w~ business.");
	    Log_Write("logs/biz_log.txt", "[%s] %s da mua business ID: %d voi gia %s.", ReturnDate(), ReturnName(playerid), id, FormatNumber(BusinessData[id][bizPrice]));
	}
	else if((id = Business_Inside(playerid)) != -1)
	{
		if(BusinessData[id][bizLocked] != 0)
			return SendErrorMessage(playerid, "Business nay da khoa.");

		if(BusinessData[id][bizType] == 5)
		{
			Business_CarMenu(playerid, id);
		}
		else
		{
			Business_PurchaseMenu(playerid, id);
		}
	}
	return 1;
}

CMD:muathucan(playerid, params[])
{
	static
		id = -1;

    if(PlayerData[playerid][pHospital] != -1 || PlayerData[playerid][pCuffed] || PlayerData[playerid][pInjured] || !IsPlayerSpawned(playerid))
		return SendErrorMessage(playerid, "Ban khong the lam dieu nay ngay bay gio.");

	if((id = Vendor_Nearest(playerid)) != -1)
	{
		switch(VendorData[id][vendorType])
		{
			case 1:
			{
				if(GetMoney(playerid) < 3)
					return SendErrorMessage(playerid, "Ban can phai co $3 tien mat.");

				if(PlayerData[playerid][pVendorTime] > 0)
					return SendErrorMessage(playerid, "Ban phai cho vai giay moi co the mua do tai Vendor.");

				if(Inventory_Count(playerid, "Buger da chin") >= 5)
					return SendErrorMessage(playerid, "Ban da co rat nhieu buger trong tui do.");

				id = Inventory_Add(playerid, "Buger da chin", 2703);

				if(id != -1)
				{
					PlayerData[playerid][pVendorTime] = 3;

					GiveMoney(playerid, -3);
					ApplyAnimation(playerid, "DEALER", "shop_pay", 4.0, 0, 0, 0, 0, 0);

					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da mua buger tai Vendor voi gia $3.", ReturnName(playerid, 0));
					ShowPlayerFooter(playerid, "~p~Burger~w~ da duoc them vao tui do.");
				}
			}
			case 2:
			{
				if(GetMoney(playerid) < 2)
					return SendErrorMessage(playerid, "Ban can phai co $2 tien mat.");

				if(PlayerData[playerid][pVendorTime] > 0)
					return SendErrorMessage(playerid, "Ban phai cho vai giay moi co the mua do tai Vendor.");

				if(Inventory_Count(playerid, "Soda") >= 10)
					return SendErrorMessage(playerid, "Ban da co rat nhieu soda trong tui do.");

				id = Inventory_Add(playerid, "Soda", 1543);

				if(id != -1)
				{
					PlayerData[playerid][pVendorTime] = 3;

					GiveMoney(playerid, -2);
					ApplyAnimation(playerid, "VENDING", "VEND_USE", 4.0, 0, 0, 0, 0, 0);

				    SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da mua soda tai Vendor voi gia $2.", ReturnName(playerid, 0));
					ShowPlayerFooter(playerid, "~p~Soda~w~ da duoc them vao tui do.");
				}
			}
		}
	}
	return 1;
}
