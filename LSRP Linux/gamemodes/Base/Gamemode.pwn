CMD:cuff(playerid, params[])
{
	new
		userid;

	if(GetFactionType(playerid) != FACTION_POLICE)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(sscanf(params, "u", userid))
		return SendSyntaxMessage(playerid, "/cuff [playerid/name]");

	if(userid == INVALID_PLAYER_ID)
		return SendErrorMessage(playerid, "Nguoi choi khong hop le.");

	if(userid == playerid)
		return SendErrorMessage(playerid, "Ban khong the cong tay chinh minh.");

	if(!IsPlayerNearPlayer(playerid, userid, 5.0))
		return SendErrorMessage(playerid, "Ban phai dung gan nguoi choi do.");

	if(PlayerData[userid][pStunned] < 0 && GetPlayerSpecialAction(userid) != SPECIAL_ACTION_HANDSUP && !IsPlayerIdle(userid))
		return SendErrorMessage(playerid, "Nguoi choi do phai gio tay hoac dau hang.");

	if(GetPlayerState(userid) != PLAYER_STATE_ONFOOT)
		return SendErrorMessage(playerid, "Nguoi choi do phai di bo.");

	if(PlayerData[userid][pCuffed])
		return SendErrorMessage(playerid, "Nguoi choi do dang bi cong tay.");

	static
		string[64];

	if(PlayerData[userid][pDrinking])
	{
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_NONE);

		DestroyPlayerProgressBar(playerid, PlayerData[playerid][pDrinkBar]);
		PlayerData[userid][pDrinking] = 0;
	}
	if(PlayerData[userid][pHoldWeapon] > 0)
	{
		HoldWeapon(userid, 0);
	}
	PlayerData[userid][pCuffed] = 1;
	SetPlayerSpecialAction(userid, SPECIAL_ACTION_CUFFED);

	format(string, sizeof(string), "Ban da bi ~r~cong tau~w~ boi %s.", ReturnName(playerid, 0));
	ShowPlayerFooter(userid, string);

	SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay cong tu tui that lung va cong tay %s.", ReturnName(playerid, 0), ReturnName(userid, 0));
	return 1;
}

