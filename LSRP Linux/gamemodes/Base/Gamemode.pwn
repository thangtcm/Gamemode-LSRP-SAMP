CMD:tog(playerid, params[])
{
	if(isnull(params))
	{
		SendSyntaxMessage(playerid, "/tog [name]");
		SendClientMessage(playerid, COLOR_YELLOW, "Lua chon: ooc, pm, faction, helper, broadcast, speedo, vip.");
		return 1;
	}
	if(!strcmp(params, "ooc", true))
	{
		if(!PlayerData[playerid][pDisableOOC])
		{
			PlayerData[playerid][pDisableOOC] = 1;
			SendServerMessage(playerid, "Ban da tat kenh OOC chat.");
		}
		else
		{
			PlayerData[playerid][pDisableOOC] = 0;
			SendServerMessage(playerid, "Ban da bat kenh OOC chat.");
		}
	}
	else if(!strcmp(params, "pm", true))
	{
		if(PlayerData[playerid][pDonator] != 0)
		{
			if(!PlayerData[playerid][pDisablePM])
			{
				PlayerData[playerid][pDisablePM] = 1;
				SendServerMessage(playerid, "Ban da tat kenh PM chat.");
			}
			else
			{
				PlayerData[playerid][pDisablePM] = 0;
				SendServerMessage(playerid, "Ban da bat kenh PM chat.");
			}
		}
		else SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");
	}
	else if(!strcmp(params, "faction", true))
	{
		if(PlayerData[playerid][pFaction] == -1)
			return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

		if(!PlayerData[playerid][pDisableFaction])
		{
			PlayerData[playerid][pDisableFaction] = 1;
			SendServerMessage(playerid, "Ban da tat kenh faction chat.");
		}
		else
		{
			PlayerData[playerid][pDisableFaction] = 0;
			SendServerMessage(playerid, "Ban da bat kenh faction chat.");
		}
	}
	else if(!strcmp(params, "helper", true))
	{
		if(!PlayerData[playerid][pHelper])
			return SendErrorMessage(playerid, "Ban khong phai la Helper.");

		if(!PlayerData[playerid][pDisableHelper])
		{
			PlayerData[playerid][pDisableHelper] = 1;
			SendServerMessage(playerid, "Ban da tat kenh helper chat.");
		}
		else
		{
			PlayerData[playerid][pDisableHelper] = 0;
			SendServerMessage(playerid, "Ban da bat kenh helper chat.");
		}
	}
	else if(!strcmp(params, "broadcast", true))
	{
		if(!PlayerData[playerid][pDisableBC])
		{
			PlayerData[playerid][pDisableBC] = 1;
			SendServerMessage(playerid, "Ban da tat kenh phat song truc tiep.");
		}
		else
		{
			PlayerData[playerid][pDisableBC] = 0;
			SendServerMessage(playerid, "Ban da bat kenh phat song truc tiep.");
		}
	}
	else if(!strcmp(params, "speedo", true))
	{
		if(!PlayerData[playerid][pDisableSpeedo])
		{
			for(new i = 34; i < 39; i++)
			{
				PlayerTextDrawHide(playerid, PlayerData[playerid][pTextdraws][i]);
			}
			PlayerData[playerid][pDisableSpeedo] = 1;
			SendServerMessage(playerid, "Ban da tat dong ho toc do.");
		}
		else
		{
			if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && IsSpeedoVehicle(GetPlayerVehicleID(playerid)))
			{
				for(new i = 34; i < 39; i++)
				{
					PlayerTextDrawShow(playerid, PlayerData[playerid][pTextdraws][i]);
				}
			}
			PlayerData[playerid][pDisableSpeedo] = 0;
			SendServerMessage(playerid, "Ban da bat dong ho toc do.");
		}
	}
	else if(!strcmp(params, "vip", true))
	{
		if(!PlayerData[playerid][pDonator])
			return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

		if(!PlayerData[playerid][pDisableDonator])
		{
			PlayerData[playerid][pDisableDonator] = 1;
			SendServerMessage(playerid, "Ban da tat kenh VIP chat.");
		}
		else
		{
			PlayerData[playerid][pDisableDonator] = 0;
			SendServerMessage(playerid, "Ban da bat kenh VIP chat.");
		}
	}
	return 1;
}

CMD:ticket(playerid, params[])
{
	static
		userid,
		price,
		reason[64];

	if(GetFactionType(playerid) != FACTION_POLICE)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(sscanf(params, "uds[64]", userid, price, reason))
		return SendSyntaxMessage(playerid, "/ticket [playerid/name] [price] [reason]");

	if(userid == INVALID_PLAYER_ID || !IsPlayerNearPlayer(playerid, userid, 5.0))
		return SendErrorMessage(playerid, "Nguoi choi khong hop le.");

	if(userid == playerid)
		return SendErrorMessage(playerid, "Ban khong the viet ve phat cho chinh minh.");

	if(price < 1 || price > 1000)
		return SendErrorMessage(playerid, "So tien khong the duoi $1 hoac cao hon $1,000.");

	new id = Ticket_Add(userid, price, reason);

	if(id != -1)
	{
		SendServerMessage(playerid, "Ban da viet ve phat cho %s voi so luong tien %s, ly do: %s", ReturnName(userid, 0), FormatNumber(price), reason);
		SendServerMessage(userid, "%s da viet ve phat cho ban voi so luong tien %s, ly do: %s", ReturnName(playerid, 0), FormatNumber(price), reason);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da viet ve phat va dua cho %s.", ReturnName(playerid, 0), ReturnName(userid, 0));
		Log_Write("logs/ticket_log.txt", "[%s] %s da viet ve phat cho %s voi so luong tien %s, ly do: %s", ReturnDate(), ReturnName(playerid, 0), FormatNumber(price), ReturnName(userid, 0), reason);
	}
	else
	{
		SendErrorMessage(playerid, "Nguoi nay da co %d ve phat.", MAX_PLAYER_TICKETS);
	}
	return 1;
}

