CMD:setstats(playerid, params[])
{
	if(PlayerData[playerid][pAdmin] < 4)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	static
		userid,
		type[16],
		amount[32];

	if(sscanf(params, "us[16]S()[32]", userid, type, amount))
	{
		SendSyntaxMessage(playerid, "/setstats [playerid/name] [name]");
		SendClientMessage(playerid, COLOR_YELLOW, "Lua chon: gender, birthdate, origin, bank, savings, hunger, thirst, level, playinghours.");
		SendClientMessage(playerid, COLOR_YELLOW, "Lua chon: job, warrants, channel, channel2, channel3, channel4, clothes1, clothes2, clothes3.");
		return 1;
	}
	if(userid == INVALID_PLAYER_ID)
		return SendErrorMessage(playerid, "Nguoi choi khong hop le.");

	if(!strcmp(type, "gender", true))
	{
		if(isnull(amount) || strval(amount) < 1 || strval(amount) > 2)
			return SendSyntaxMessage(playerid, "/setstats [playerid/name] [gender] [1: trai - 2: gai]");

		PlayerData[userid][pGender] = strval(amount);

		if(PlayerData[userid][pGender] == 1)
			SendAdminAction(playerid, "Ban da set %s gioi tinh trai.", ReturnName(userid, 0));

		else if(PlayerData[userid][pGender] == 2)
			SendAdminAction(playerid, "Ban da set %s gioi tinh gai.", ReturnName(userid, 0));
	}
	else if(!strcmp(type, "birthdate", true))
	{
		if(isnull(amount) || strlen(amount) > 24)
			return SendSyntaxMessage(playerid, "/setstats [playerid/name] [birthdate] [birth date]");

		format(PlayerData[userid][pBirthdate], 24, amount);
		SendAdminAction(playerid, "Ban da set %s ngay sinh \"%s\".", ReturnName(userid, 0), amount);
	}
	else if(!strcmp(type, "origin", true))
	{
		if(isnull(amount) || strlen(amount) > 32)
			return SendSyntaxMessage(playerid, "/setstats [playerid/name] [origin] [new origin]");

		format(PlayerData[userid][pOrigin], 32, amount);
		SendAdminAction(playerid, "Ban da set %s noi song \"%s\".", ReturnName(userid, 0), amount);
	}
	else if(!strcmp(type, "bank", true))
	{
	    if(isnull(amount))
