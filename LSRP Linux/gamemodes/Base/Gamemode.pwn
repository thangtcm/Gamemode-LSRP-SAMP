CMD:signcheck(playerid, params[])
{
	new payday;

	if(ATM_Nearest(playerid) == -1 && !IsPlayerInRangeOfPoint(playerid, 2, 1104.9641, 1529.5609, 4452.4810))
		return SendErrorMessage(playerid, "Ban khong dung gan dia diem nhan paycheck hoac ATM.");

	if(sscanf(params, "d", payday))
		return SendSyntaxMessage(playerid, "/signcheck [Payday ID]");

	if(PlayerData[playerid][pPaydayID] == -1)
		return SendErrorMessage(playerid, "Payday ID ban nhap khong dung.");

	if(payday != PlayerData[playerid][pPaydayID])
		return SendErrorMessage(playerid, "Payday ID ban nhap khong dung.");

	Payday(playerid);
	SendClientMessage(playerid, COLOR_YELLOW, "Ban da nhan payday thanh cong.");
	return 1;
}

