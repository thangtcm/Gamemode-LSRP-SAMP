CMD:setstyle(playerid, params[])
{
	new type[30], style[90];

	if(sscanf(params, "s[30]S()[90]", type, style))
	{
		SendSyntaxMessage(playerid, "/setstyle [type] [style]");
		SendClientMessage(playerid, COLOR_YELLOW, "Lua chon: chatstyle, walkstyle.");
		return 1;
	}
	if(!strcmp(type, "chatstyle"))
	{
		new chatstyle;

		if(sscanf(style, "d", chatstyle))
			return SendSyntaxMessage(playerid, "/setstyle [chatstyle] [1-8]");

		if(!(1 <= chatstyle <= 8))
			return SendSyntaxMessage(playerid, "/setstyle [chatstyle] [1-8]");

		PlayerData[playerid][pChatStyle] = chatstyle;
		SendClientMessageEx(playerid, COLOR_YELLOW, "Ban da thay doi chat style %d.", chatstyle);
	}
	else if(!strcmp(type, "walkstyle"))
	{
		new walkstyle;

		if(sscanf(style, "d", walkstyle))
		{
			SendSyntaxMessage(playerid, "/setstyle [walkstyle] [1-16]");
			SendClientMessage(playerid, COLOR_YELLOW, "Random: 7, 8.");
			SendClientMessage(playerid, COLOR_YELLOW, "Binh thuong: 1, 2, 6, 9, 11.");
			SendClientMessage(playerid, COLOR_YELLOW, "Gia/Map: 3, 4, 5, 10.");
			SendClientMessage(playerid, COLOR_YELLOW, "Con gai: 12, 13, 14, 15, 16.");
			return 1;
		}

		if(!(1 <= walkstyle <= 16))
		{
			SendSyntaxMessage(playerid, "/setstyle [walkstyle] [1-16]");
			SendClientMessage(playerid, COLOR_YELLOW, "Random: 7, 8.");
			SendClientMessage(playerid, COLOR_YELLOW, "Binh thuong: 1, 2, 6, 9, 11.");
			SendClientMessage(playerid, COLOR_YELLOW, "Gia/Map: 3, 4, 5, 10.");
			SendClientMessage(playerid, COLOR_YELLOW, "Con gai: 12, 13, 14, 15, 16.");
			return 1;
		}
		PlayerData[playerid][pWalkStyle] = walkstyle;
		SendClientMessageEx(playerid, COLOR_YELLOW, "Ban da thay doi walk style %d.", walkstyle);
	}
	return 1;
}

