stock GetElapsedTime(time, &hours, &minutes, &seconds)
{
	hours = 0;
	minutes = 0;
	seconds = 0;

	if (time >= 3600)
	{
		hours = (time / 3600);
		time -= (hours * 3600);
	}
	while (time >= 60)
	{
		minutes++;
		time -= 60;
	}
	return (seconds = time);
}

stock GetDuration(time)
{
	new
		str[32];

	if(time < 0 || time == gettime())
	{
		format(str, sizeof(str), "Khong bao gio");
		return str;
	}
	else if (time < 60)
		format(str, sizeof(str), "%d giay", time);

	else if (time >= 0 && time < 60)
		format(str, sizeof(str), "%d giay", time);

	else if (time >= 60 && time < 3600)
		format(str, sizeof(str), (time >= 120) ? ("%d phut") : ("%d phut"), time / 60);

	else if (time >= 3600 && time < 86400)
		format(str, sizeof(str), (time >= 7200) ? ("%d tieng") : ("%d tieng"), time / 3600);

	else if (time >= 86400 && time < 2592000)
 		format(str, sizeof(str), (time >= 172800) ? ("%d ngay") : ("%d ngay"), time / 86400);

	else if (time >= 2592000 && time < 31536000)
 		format(str, sizeof(str), (time >= 5184000) ? ("%d thang") : ("%d thang"), time / 2592000);

	else if (time >= 31536000)
		format(str, sizeof(str), (time >= 63072000) ? ("%d nam") : ("%d nam"), time / 31536000);

	strcat(str, " truoc");

	return str;
}

