CMD:baocao(playerid, params[])
{
	new reportid = -1;

	if(isnull(params))
	{
		SendSyntaxMessage(playerid, "/baocao [reason]");
		SendClientMessage(playerid, COLOR_LIGHTRED, "Xin vui long bao cao voi muc dich hop le.");
		return 1;
	}
	if(Report_GetCount(playerid) > 5)
		return SendErrorMessage(playerid, "Ban dang co 5 bao cao chua duoc duyet.");

	if(PlayerData[playerid][pReportTime] >= gettime())
		return SendErrorMessage(playerid, "Ban phai cho %d giay moi co the tiep tuc bao cao.", PlayerData[playerid][pReportTime] - gettime());

	if((reportid = Report_Add(playerid, params)) != -1)
	{
		ShowPlayerFooter(playerid, "~g~Bao cao~w~ cua ban da duoc dua len Administrator.");

		foreach(new i : Player)
		{
			if(PlayerData[i][pAdmin] > 0 && PlayerData[i][pAdminDuty])
			{
				SendClientMessageEx(i, COLOR_LIGHTYELLOW, "[Bao cao %d]: %s (ID: %d), ly do: %s", reportid, ReturnName(playerid, 0), playerid, params);
			}
		}
		PlayerData[playerid][pReportTime] = gettime() + 15;
		SendServerMessage(playerid, "Bao cao cua ban da duoc gui len Administrator.");
	}
	else
	{
		SendErrorMessage(playerid, "Dach sach bao cao da day vui long doi chut.");
	}
	return 1;
}

CMD:xembaocao(playerid, params[])
