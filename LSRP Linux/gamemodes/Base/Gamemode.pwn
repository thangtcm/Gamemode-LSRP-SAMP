CMD:twithdraw(playerid, params[])
{
	static
		amount;

	if(GetFactionType(playerid) != FACTION_GOV)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(sscanf(params, "d", amount))
		return SendSyntaxMessage(playerid, "/twithdraw [amount] (%s co san)", FormatNumber(g_TaxVault));

	if(!IsPlayerInCityHall(playerid))
		return SendErrorMessage(playerid, "Ban phai ben trong CityHall.");

	if(amount < 1 || amount > g_TaxVault)
		return SendErrorMessage(playerid, "So luong tien khong hop le.");

	if(PlayerData[playerid][pFactionRank] < FactionData[PlayerData[playerid][pFaction]][factionRanks] - 1)
		return SendErrorMessage(playerid, "Ban phai tu rank %d moi co the su dung lenh nay.", FactionData[PlayerData[playerid][pFaction]][factionRanks] - 1);

	Tax_AddMoney(-amount);

	GiveMoney(playerid, amount);
	SendServerMessage(playerid, "Ban da rut %s tu ket sat (%s co san).", FormatNumber(amount), FormatNumber(g_TaxVault));

	SendAdminAlert(COLOR_LIGHTRED, "[Administrator]: %s da rut %s tu ket sat.", ReturnName(playerid, 0), FormatNumber(amount));
	Log_Write("logs/tax_vault.txt", "[%s] %s da rut %s tu ket sat.", ReturnDate(), ReturnName(playerid, 0), FormatNumber(amount));
	return 1;
}

CMD:tdeposit(playerid, params[])
{
