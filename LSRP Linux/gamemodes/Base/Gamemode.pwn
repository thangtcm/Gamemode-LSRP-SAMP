CMD:lastlogged(playerid, params[])
{
	if(isnull(params) || strlen(params) > 24)
		return SendSyntaxMessage(playerid, "/lastlogged [username]");

	static
		query[128];

	format(query, sizeof(query), "SELECT `LoginDate` FROM `accounts` WHERE `Username` = '%s'", SQL_ReturnEscaped(params));
	mysql_tquery(g_iHandle, query, "OnLoginDate", "ds", playerid, params);

 	return 1;
}

CMD:v(playerid, params[])
{
	if(isnull(params))
	{
		SendSyntaxMessage(playerid, "/v [Lua chon]");
		SendClientMessage(playerid, COLOR_YELLOW, "Lua chon: Park, Lock.");
		SendClientMessage(playerid, COLOR_YELLOW, "Lua chon: Engine, Lights, Hood, Trunk, ViewTrunk, Window, Windows.");
	}
	if(!strcmp(params, "park", true))
	{
		new
			carid = GetPlayerVehicleID(playerid);

		if(!carid)
			return SendErrorMessage(playerid, "Ban phai ben trong phuong tien.");

		if(IsVehicleImpounded(carid))
			return SendErrorMessage(playerid, "Phuong tien nay da bi giam va khong the su dung.");

		if((carid = Car_GetID(carid)) != -1 && Car_IsOwner(playerid, carid))
		{
			if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER)
				return SendErrorMessage(playerid, "Ban phai la nguoi lai xe.");

			static
				g_arrSeatData[10] = {INVALID_PLAYER_ID, ...},
				g_arrDamage[4],
				Float:health,
				seatid;

			for(new i = 0; i < 14; i++)
			{
				CarData[carid][carMods][i] = GetVehicleComponentInSlot(CarData[carid][carVehicle], i);
			}
			GetVehicleDamageStatus(CarData[carid][carVehicle], g_arrDamage[0], g_arrDamage[1], g_arrDamage[2], g_arrDamage[3]);
			GetVehicleHealth(CarData[carid][carVehicle], health);

			foreach(new i : Player) if(IsPlayerInVehicle(i, CarData[carid][carVehicle]))
			{
				seatid = GetPlayerVehicleSeat(i);

				g_arrSeatData[seatid] = i;
			}
			GetVehiclePos(CarData[carid][carVehicle], CarData[carid][carPos][0], CarData[carid][carPos][1], CarData[carid][carPos][2]);
			GetVehicleZAngle(CarData[carid][carVehicle], CarData[carid][carPos][3]);

			Car_Spawn(carid);
			Car_Save(carid);

			SendServerMessage(playerid, "Ban da dau phuong tien %s.", ReturnVehicleName(CarData[carid][carVehicle]));

			UpdateVehicleDamageStatus(CarData[carid][carVehicle], g_arrDamage[0], g_arrDamage[1], g_arrDamage[2], g_arrDamage[3]);
			SetVehicleHealth(CarData[carid][carVehicle], health);

			for(new i = 0; i < sizeof(g_arrSeatData); i++) if(g_arrSeatData[i] != INVALID_PLAYER_ID)
			{
				PutPlayerInVehicle(g_arrSeatData[i], CarData[carid][carVehicle], i);

				g_arrSeatData[i] = INVALID_PLAYER_ID;
			}
		}
		else SendErrorMessage(playerid, "Ban khong phai chu so huu phuong tien nay.");
	}
	else if(!strcmp(params, "lock", true))
	{
		static id = -1;

		if((id = Car_Nearest(playerid)) != -1)
		{
			static
				engine,
				lights,
				alarm,
				doors,
				bonnet,
				boot,
				objective;

			GetVehicleParamsEx(CarData[id][carVehicle], engine, lights, alarm, doors, bonnet, boot, objective);

			if(Car_IsOwner(playerid, id) || (PlayerData[playerid][pFaction] != -1 && CarData[id][carFaction] == GetFactionType(playerid)))
			{
				if(!CarData[id][carLocked])
				{
					CarData[id][carLocked] = true;
					Car_Save(id);

					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da khoa phuong tien %s.", ReturnName(playerid, 0), ReturnVehicleName(CarData[id][carVehicle]));
					PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
					ShowPlayerFooter(playerid, "Ban da ~r~khoa~w~ phuong tien.");

					SetVehicleParamsEx(CarData[id][carVehicle], engine, lights, alarm, 1, bonnet, boot, objective);
				}
				else
				{
					CarData[id][carLocked] = false;
					Car_Save(id);

					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da mo khoa phuong tien %s.", ReturnName(playerid, 0), ReturnVehicleName(CarData[id][carVehicle]));
					PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
					ShowPlayerFooter(playerid, "Ban da ~g~mo khoa~w~ phuong tien.");

					SetVehicleParamsEx(CarData[id][carVehicle], engine, lights, alarm, 0, bonnet, boot, objective);
				}
			}
		}
		else SendErrorMessage(playerid, "Ban khong dung gan dung phuong tien so huu cua ban.");
	}

	else if(!strcmp(params, "engine", true))
	{
		new vehicleid = GetPlayerVehicleID(playerid);

		if(!IsEngineVehicle(vehicleid))
			return SendErrorMessage(playerid, "Ban khong co tren phuong tien.");

		if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER)
			return SendErrorMessage(playerid, "Ban phai la nguoi lai xe.");

		if(CoreVehicles[vehicleid][vehFuel] < 1)
			return SendErrorMessage(playerid, "Xang trong phuong tien da het.");

		if(ReturnVehicleHealth(vehicleid) <= 300)
			return SendErrorMessage(playerid, "Phuong tien nay da bi hong rat nang.");

		switch(GetEngineStatus(vehicleid))
		{
			case false:
			{
				SetEngineStatus(vehicleid, true);
				ShowPlayerFooter(playerid, "Ban da ~g~khoi dong~w~ phuong tien.");
				SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da cam chia khoa va khoi dong co phuong tien %s.", ReturnName(playerid, 0), ReturnVehicleName(vehicleid));
			}
			case true:
			{
				SetEngineStatus(vehicleid, false);
				ShowPlayerFooter(playerid, "Ban da dung ~r~khoi dong~w~ phuong tien.");
				SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay chia khoa ra va tat dong co phuong tien %s.", ReturnName(playerid, 0), ReturnVehicleName(vehicleid));
			}
		}
	}
	else if(!strcmp(params, "lights", true))
	{
		new vehicleid = GetPlayerVehicleID(playerid);

		if(!IsEngineVehicle(vehicleid))
			return SendErrorMessage(playerid, "Ban khong co tren phuong tien.");

		if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER)
			return SendErrorMessage(playerid, "Ban phai la nguoi lai xe.");

		switch(GetLightStatus(vehicleid))
		{
			case false:
			{
				SetLightStatus(vehicleid, true);
				ShowPlayerFooter(playerid, "Ban da ~g~bat~w~ den phuong tien.");
			}
			case true:
			{
				SetLightStatus(vehicleid, false);
				ShowPlayerFooter(playerid, "Ban da ~r~tat~w~ den phuong tien.");
			}
		}
	}
	else if(!strcmp(params, "hood", true))
	{
		for(new i = 1; i != MAX_VEHICLES; i++) if(IsValidVehicle(i) && IsPlayerNearHood(playerid, i))
		{
			if(!IsDoorVehicle(i))
				return SendErrorMessage(playerid, "Phuong tien nay khong co mui.");

			if(!GetHoodStatus(i))
			{
				SetHoodStatus(i, true);

				SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da mo mui phuong tien.", ReturnName(playerid, 0));
				ShowPlayerFooter(playerid, "Ban da ~g~mo~w~ mui phuong tien.");
			}
			else
			{
				SetHoodStatus(i, false);

				SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da dong mui phuong tien.", ReturnName(playerid, 0));
				ShowPlayerFooter(playerid, "Ban da ~g~dong~w~ mui phuong tien.");
			}
			return 1;
		}
		SendErrorMessage(playerid, "Ban khong dung gan phuong tien nao.");
		return 1;
	}
	else if(!strcmp(params, "trunk", true))
	{
		for(new i = 1; i != MAX_VEHICLES; i++) if(IsValidVehicle(i) && IsPlayerNearBoot(playerid, i))
		{
			if(!IsDoorVehicle(i))
				return SendErrorMessage(playerid, "Phuong tien nay khong co cop.");

			if(!GetTrunkStatus(i))
			{
				SetTrunkStatus(i, true);

				SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da mo cop phuong tien.", ReturnName(playerid, 0));
				ShowPlayerFooter(playerid, "Ban da ~g~mo~w~ cop.");
			}
			else
			{
				SetTrunkStatus(i, false);

				SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da dong cop phuong tien.", ReturnName(playerid, 0));
				ShowPlayerFooter(playerid, "Ban da ~g~dong~w~ cop.");
			}
			return 1;
		}
		SendErrorMessage(playerid, "Ban khong dung gan phuong tien nao.");
		return 1;
	}
	else if(!strcmp(params, "viewtrunk", true))
	{
		new
			id = -1;

		if((id = Car_Nearest(playerid)) != -1)
		{
			if(IsPlayerNearBoot(playerid, id))
				return SendErrorMessage(playerid, "Ban khong dung gan cop xe.");

			if(!GetTrunkStatus(CarData[id][carVehicle]))
				return SendErrorMessage(playerid, "Cop xe phai mo moi co the lay item.");

			if(IsVehicleImpounded(CarData[id][carVehicle]))
				return SendErrorMessage(playerid, "Phuong tien nay da bi giam va khong the su dung.");

			if(IsPlayerInAnyVehicle(playerid))
				return SendErrorMessage(playerid, "Ban phai ra khoi phuong tien truoc.");

			if(!IsDoorVehicle(CarData[id][carVehicle]))
				return SendErrorMessage(playerid, "Phuong tien nay khong co cop.");

			if(CarData[id][carLocked])
				return SendErrorMessage(playerid, "Phuong tien da khoa.");

			Car_ShowTrunk(playerid, id);
		}
		else SendErrorMessage(playerid, "Ban khong dung gan phuong tien.");
		return 1;
	}
	else if(!strcmp(params, "window", true))
	{
		new driver, passenger, backleft, backright,
			seat = GetPlayerVehicleSeat(playerid);

		GetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), driver, passenger, backleft, backright);
		if(IsPlayerInAnyVehicle(playerid))
		{
			if(seat == 0)
			{
				if(Windows[GetPlayerVehicleID(playerid)][0] == 0)
				{
					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da mo cua so.", ReturnName(playerid, 0));
					Windows[GetPlayerVehicleID(playerid)][0] = 1;
					SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), 0, passenger, backleft, backright);
				}
				else if(Windows[GetPlayerVehicleID(playerid)][0] == 1)
				{
					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da dong cua so.", ReturnName(playerid, 0));
				    Windows[GetPlayerVehicleID(playerid)][0] = 0;
				    SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), 1, passenger, backleft, backright);
				}
			}
			else if(seat == 1)
			{
				if(Windows[GetPlayerVehicleID(playerid)][1] == 0)
				{
					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da mo cua so.", ReturnName(playerid, 0));
					Windows[GetPlayerVehicleID(playerid)][1] = 1;
					SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), driver, 0, backleft, backright);
				}
				else if(Windows[GetPlayerVehicleID(playerid)][1] == 1)
				{
					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da dong cua so.", ReturnName(playerid, 0));
					Windows[GetPlayerVehicleID(playerid)][1] = 0;
					SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), driver, 1, backleft, backright);
				}
			}
			else if(seat == 2)
			{
				if(Windows[GetPlayerVehicleID(playerid)][2] == 0)
				{
					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da mo cua so.", ReturnName(playerid, 0));
					Windows[GetPlayerVehicleID(playerid)][2] = 1;
					SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), driver, passenger, 0, backright);
				}
				else if(Windows[GetPlayerVehicleID(playerid)][2] == 1)
				{
					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da dong cua so.", ReturnName(playerid, 0));
				    Windows[GetPlayerVehicleID(playerid)][2] = 0;
					SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), driver, passenger, 1, backright);
				}
			}
			else if(seat == 3)
			{
				if(Windows[GetPlayerVehicleID(playerid)][3] == 0)
				{
					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da mo cua so.", ReturnName(playerid, 0));
					Windows[GetPlayerVehicleID(playerid)][3] = 1;
					SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), driver, passenger, backleft, 0);
				}
				else if(Windows[GetPlayerVehicleID(playerid)][3] == 1)
				{
					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da dong cua so.", ReturnName(playerid, 0));
					Windows[GetPlayerVehicleID(playerid)][3] = 0;
					SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), driver, passenger, backleft, 1);
				}
			}
		}
	}
	else if(!strcmp(params, "windows", true))
	{
		new vehicleid = GetPlayerVehicleID(playerid);

		if(!IsWindowedVehicle(vehicleid))
			return SendErrorMessage(playerid, "Ban khong the mo cua so voi phuong tien nay.");

		switch(CoreVehicles[vehicleid][vehWindowsDown])
		{
			case false:
			{
				CoreVehicles[vehicleid][vehWindowsDown] = true;
				ShowPlayerFooter(playerid, "Ban da ~g~mo~w~ tat ca cua so.");
				SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da mo tat ca cua so.", ReturnName(playerid, 0));
				SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), 0, 0, 0, 0);
			}
			case true:
			{
				CoreVehicles[vehicleid][vehWindowsDown] = false;
				ShowPlayerFooter(playerid, "Ban da ~r~dong~w~ tat ca cua so.");
				SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da dong tat ca cua so.", ReturnName(playerid, 0));
				SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), 1, 1, 1, 1);
			}
		}
	}
	return 1;
}

CMD:placegun(playerid, params[])
{
	new
		id = -1,
		slot,
		str[128];

	if((id = GetNearestVehicle(playerid)) != -1)
	{
		if(!IsValidVehicle(id))
			return SendErrorMessage(playerid, "Ban khong dung gan phuong tien nao.");

		if(IsPlayerInAnyVehicle(playerid))
			return SendErrorMessage(playerid, "Ban phai ra khoi phuong tien moi co the cat vu khi.");

		if(!IsDoorVehicle(id))
			return SendErrorMessage(playerid, "Phuong tien nay khong co cop.");

		if(!GetWeapon(playerid))
			return SendErrorMessage(playerid, "Ban phai cam vu khi tren tay de cat vao phuong tien.");

		if(sscanf(params, "d", slot))
			return SendSyntaxMessage(playerid, "/placegun [slot].");

		if(slot < 1 || slot > 5)
			return SendErrorMessage(playerid, "Slot phai tu 1 den 5.");

		if(slot == 1)
		{
			if(CoreVehicles[id][vehWeapons][0] != 0)
				return SendErrorMessage(playerid, "Slot nay da co vu khi, su dung [/checkgun] de xem.");

			CoreVehicles[id][vehWeapons][0] = GetWeapon(playerid);
			CoreVehicles[id][vehAmmo][0] = GetPlayerAmmo(playerid);

			ResetWeapon(playerid, CoreVehicles[id][vehWeapons][0]);
			format(str, sizeof(str), "> %s da cat vu khi %s vao trong cop xe.", ReturnName(playerid, 0), ReturnWeaponName(CoreVehicles[id][vehWeapons][0]));
			SetPlayerChatBubble(playerid, str, COLOR_PURPLE, 30.0, 10000);
			SendClientMessage(playerid, COLOR_PURPLE, str);
			return 1;
		}
		else if(slot == 2)
		{
			if(CoreVehicles[id][vehWeapons][1] != 0)
				return SendErrorMessage(playerid, "Slot nay da co vu khi, su dung [/checkgun] de xem.");

			CoreVehicles[id][vehWeapons][1] = GetWeapon(playerid);
			CoreVehicles[id][vehAmmo][1] = GetPlayerAmmo(playerid);

			ResetWeapon(playerid, CoreVehicles[id][vehWeapons][1]);
			format(str, sizeof(str), "* %s da cat vu khi %s vao trong cop xe.", ReturnName(playerid, 0), ReturnWeaponName(CoreVehicles[id][vehWeapons][1]));
			SetPlayerChatBubble(playerid, str, COLOR_PURPLE, 30.0, 10000);
			SendClientMessage(playerid, COLOR_PURPLE, str);
			return 1;
		}
		else if(slot == 3)
		{
			if(CoreVehicles[id][vehWeapons][2] != 0)
				return SendErrorMessage(playerid, "Slot nay da co vu khi, su dung [/checkgun] de xem.");

			CoreVehicles[id][vehWeapons][2] = GetWeapon(playerid);
			CoreVehicles[id][vehAmmo][2] = GetPlayerAmmo(playerid);

			ResetWeapon(playerid, CoreVehicles[id][vehWeapons][2]);
			format(str, sizeof(str), "* %s da cat vu khi %s vao trong cop xe.", ReturnName(playerid, 0), ReturnWeaponName(CoreVehicles[id][vehWeapons][2]));
			SetPlayerChatBubble(playerid, str, COLOR_PURPLE, 30.0, 10000);
			SendClientMessage(playerid, COLOR_PURPLE, str);
			return 1;
		}
		else if(slot == 4)
		{
			if(CoreVehicles[id][vehWeapons][3] != 0)
				return SendErrorMessage(playerid, "Slot nay da co vu khi, su dung [/checkgun] de xem.");

			CoreVehicles[id][vehWeapons][3] = GetWeapon(playerid);
			CoreVehicles[id][vehAmmo][3] = GetPlayerAmmo(playerid);

			ResetWeapon(playerid, CoreVehicles[id][vehWeapons][3]);
			format(str, sizeof(str), "* %s da cat vu khi %s vao trong cop xe.", ReturnName(playerid, 0), ReturnWeaponName(CoreVehicles[id][vehWeapons][3]));
			SetPlayerChatBubble(playerid, str, COLOR_PURPLE, 30.0, 10000);
			SendClientMessage(playerid, COLOR_PURPLE, str);
			return 1;
		}
		else if(slot == 5)
		{
			if(CoreVehicles[id][vehWeapons][4] != 0)
				return SendErrorMessage(playerid, "Slot nay da co vu khi, su dung [/checkgun] de xem.");

			CoreVehicles[id][vehWeapons][4] = GetWeapon(playerid);
			CoreVehicles[id][vehAmmo][4] = GetPlayerAmmo(playerid);

			ResetWeapon(playerid, CoreVehicles[id][vehWeapons][4]);
			format(str, sizeof(str), "* %s da cat vu khi %s vao trong cop xe.", ReturnName(playerid, 0), ReturnWeaponName(CoreVehicles[id][vehWeapons][4]));
			SetPlayerChatBubble(playerid, str, COLOR_PURPLE, 30.0, 10000);
			SendClientMessage(playerid, COLOR_PURPLE, str);
			return 1;
		}
	}
	return 1;
}

