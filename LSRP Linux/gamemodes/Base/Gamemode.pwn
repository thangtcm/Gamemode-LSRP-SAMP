	if(IsPlayerInAnyVehicle(playerid) || vehicleid == INVALID_VEHICLE_ID)
		return SendErrorMessage(playerid, "Ban khong dung gan phuong tien.");

	if(!Inventory_HasItem(playerid, "Binh xang"))
		return SendErrorMessage(playerid, "Ban khong co binh xang.");

	if(GetEngineStatus(vehicleid))
		return SendErrorMessage(playerid, "Ban phai tat dong co phuong tien.");

	if(CoreVehicles[vehicleid][vehFuel] > 95)
		return SendErrorMessage(playerid, "Phuong tien nay khong can them xang.");

