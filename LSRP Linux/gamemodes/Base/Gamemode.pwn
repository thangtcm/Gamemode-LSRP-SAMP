			CoreVehicles[id][vehWeapons][3] = 0;
			CoreVehicles[id][vehAmmo][3] = 0;

			format(str, sizeof(str), "* %s da lay vu khi %s tu trong cop xe.", ReturnName(playerid, 0), ReturnWeaponName(CoreVehicles[id][vehWeapons][3]));
			SetPlayerChatBubble(playerid, str, COLOR_PURPLE, 30.0, 10000);
			SendClientMessage(playerid, COLOR_PURPLE, str);
			return 1;
		}
		else if(slot == 5)
		{
			if(CoreVehicles[id][vehWeapons][4] == 0)
				return SendErrorMessage(playerid, "Slot nay khong co vu khi, su dung [/checkgun] de xem.");

			GiveWeaponToPlayer(playerid, CoreVehicles[id][vehWeapons][4], CoreVehicles[id][vehAmmo][4]);
			CoreVehicles[id][vehWeapons][4] = 0;
			CoreVehicles[id][vehAmmo][4] = 0;

			format(str, sizeof(str), "* %s da lay vu khi %s tu trong cop xe.", ReturnName(playerid, 0), ReturnWeaponName(CoreVehicles[id][vehWeapons][4]));
			SetPlayerChatBubble(playerid, str, COLOR_PURPLE, 30.0, 10000);
			SendClientMessage(playerid, COLOR_PURPLE, str);
			return 1;
		}
	}
	return 1;
}

CMD:checkgun(playerid, params[])
{
	new
		id = -1,
		string[128];

	if((id = GetNearestVehicle(playerid)) != -1)
	{
		if(!IsValidVehicle(id))
			return SendErrorMessage(playerid, "Ban khong dung gan phuong tien nao.");

		if(!IsPlayerInAnyVehicle(playerid))
			return SendErrorMessage(playerid, "Ban phai len phuong tien moi co the xem vu khi trong cop xe.");

		for(new i = 0; i < 5; i++)
		{
			if(!CoreVehicles[id][vehWeapons][i])
				format(string, sizeof(string), "%s%d: Empty Slot\n", string, i + 1);

			else format(string, sizeof(string), "%s%d: Vu khi: %s - Dan: %d\n", string, i + 1, ReturnWeaponName(CoreVehicles[id][vehWeapons][i]), CoreVehicles[id][vehAmmo][i]);
		}
		Dialog_Show(playerid, ShowGuns, DIALOG_STYLE_LIST, "Thung vu khi", string, "Lua chon", "Huy bo");
	}
	return 1;
}

