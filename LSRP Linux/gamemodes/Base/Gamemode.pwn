CMD:sms(playerid, params[])
{
	if(!Inventory_HasItem(playerid, "Dien thoai"))
		return SendErrorMessage(playerid, "Ban khong co dien thoai.");

	if(PlayerData[playerid][pPhoneOff])
		return SendErrorMessage(playerid, "Ban phai mo nguon dien thoai.");

	static
		targetid,
		number,
		text[128];

	if(sscanf(params, "ds[128]", number, text))
		return SendSyntaxMessage(playerid, "/sms [phone number] [message]");

	if(!number)
		return SendErrorMessage(playerid, "So dien thoai khong hop le.");

	if((targetid = GetNumberOwner(number)) != INVALID_PLAYER_ID)
	{
		if(targetid == playerid)
			return SendErrorMessage(playerid, "Ban khong the gui tin nhan cho chinh minh.");

		if(PlayerData[targetid][pPhoneOff])
			return SendErrorMessage(playerid, "Dien thoai nguoi do da tat nguon.");

        GiveMoney(playerid, -1);
		ShowPlayerFooter(playerid, "Ban da ~r~gui tin nhan~w~.");

		SendClientMessageEx(targetid, COLOR_YELLOW, "[SMS]: %s - %s (%d)", text, ReturnName(playerid, 0), PlayerData[playerid][pPhone]);
		SendClientMessageEx(playerid, COLOR_YELLOW, "[SMS]: %s - %s (%d)", text, ReturnName(playerid, 0), PlayerData[playerid][pPhone]);

        PlayerPlaySoundEx(targetid, 21001);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay dien thoai ra va gui tin nhan cho ai do.", ReturnName(playerid, 0));
	}
	else
	{
	    SendErrorMessage(playerid, "So dien thoai khong hop le.");
	}
	return 1;
}

