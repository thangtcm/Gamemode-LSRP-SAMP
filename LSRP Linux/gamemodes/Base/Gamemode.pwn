forward MinuteCheck();
public MinuteCheck()
{
	static
		str[128];

    foreach(new i : Player)
	{
		if(!PlayerData[i][pLogged] && !PlayerData[i][pCharacter])
			continue;

		PlayerData[i][pMinutes]++;

		if(PlayerData[i][pMinutes] == 3600)
		{
			new paydayid = random(900000) + 10000;

			PlayerData[i][pPaydayID] = paydayid;
			format(str, sizeof(str), "~g~Payday ID~w~: %d", paydayid);
			PlayerTextDrawShow(i, PlayerData[i][pTextdraws][83]);
			PlayerTextDrawSetString(i, PlayerData[i][pTextdraws][83], str);
			PlayerPlaySound(i, 1058, 0.0, 0.0, 0.0);
			SendClientMessage(i, COLOR_LIGHTGREEN, "Ban da online du gio choi, ban co the den ngan hang hoac ATM de nhan paycheck.");
			SendClientMessage(i, COLOR_LIGHTGREEN, "5 phut sau neu ban khong nhan he thong se tu dong huy paycheck cua ban.");
		}
		else if(PlayerData[i][pMinutes] == 3605)
		{
			PlayerData[i][pMinutes] = 0;
			PlayerData[i][pPaydayID] = -1;
			SendClientMessage(i, COLOR_LIGHTGREEN, "Paycheck cua ban da tu dong huy.");
			PlayerTextDrawHide(i, PlayerData[i][pTextdraws][83]);
		}
	}
	for(new i = 0; i != MAX_DRUG_PLANTS; i++) if(PlantData[i][plantExists] && PlantData[i][plantDrugs] < Plant_MaxGrams(PlantData[i][plantType]))
	{
		PlantData[i][plantDrugs]++;

		Plant_Refresh(i);
		Plant_Save(i);
	}
	return 1;
}

