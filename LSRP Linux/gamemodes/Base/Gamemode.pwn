Dialog:DateBirth(playerid, response, listitem, inputtext[])
{
	if(response)
	{
		new
			iDay,
			iMonth,
			iYear,
			str[64];

		static const
			arrMonthDays[] = {31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};

		if(sscanf(inputtext, "p</>ddd", iDay, iMonth, iYear))
		{
			Dialog_Show(playerid, DateBirth, DIALOG_STYLE_INPUT, "Ngay sinh", "Ban phai nhap ngay sinh cua ban vi du ngay/thang/nam.\n\nXin vui long nhap ngay sinh (DD/MM/YYYY):", "Xac nhan", "Huy bo");
		}
		else if(iYear < 1900 || iYear > 2018)
		{
			Dialog_Show(playerid, DateBirth, DIALOG_STYLE_INPUT, "Ngay sinh", "Nam sinh cua ban khong hop le.\n\nXin vui long nhap ngay sinh (DD/MM/YYYY):", "Xac nhan", "Huy bo");
		}
		else if(iMonth < 1 || iMonth > 12)
		{
			Dialog_Show(playerid, DateBirth, DIALOG_STYLE_INPUT, "Ngay sinh", "Thang sinh cua ban khong hop le.\n\nXin vui long nhap thang sinh (DD/MM/YYYY):", "Xac nhan", "Huy bo");
		}
		else if(iDay < 1 || iDay > arrMonthDays[iMonth - 1])
		{
			Dialog_Show(playerid, DateBirth, DIALOG_STYLE_INPUT, "Ngay sinh", "Ngay sinh cua ban khong hop le.\n\nXin vui long nhap ngay sinh (DD/MM/YYYY):", "Xac nhan", "Huy bo");
		}
		else
		{
			format(PlayerData[playerid][pBirthdate], 24, inputtext);

			format(str, sizeof(str), "~r~Ngay sinh:~w~ %s", inputtext);
			PlayerTextDrawSetString(playerid, PlayerData[playerid][pTextdraws][15], str);
		}
	}
	return 1;
}

