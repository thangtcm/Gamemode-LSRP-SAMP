CMD:bshipment(playerid, params[])
{
	new id = (Business_Inside(playerid) == -1) ? (Business_Nearest(playerid)) : (Business_Inside(playerid));

	if(id == -1 || !Business_IsOwner(playerid, id))
		return SendErrorMessage(playerid, "Ban khong dung gan cua hang.");

	if(BusinessData[id][bizType] == 5)
		return SendErrorMessage(playerid, "Cua hang cua ban khong the goi van chuyen hang.");

	if(BusinessData[id][bizShipment])
	{
		foreach(new i : Player) if(PlayerData[i][pShipment] == id)
		{
			CancelShipment(i);
			SendServerMessage(i, "Yeu cau van chuyen hang da bi huy.");
		}
		BusinessData[id][bizShipment] = 0;
		Business_Save(id);

		SendServerMessage(playerid, "Cua hang cua ban da huy yeu cau van chuyen hang.");
		SendJobMessage(1, COLOR_YELLOW, "** %s da huy yeu cau van chuyen hang toi cua hang %s. **", ReturnName(playerid, 0), BusinessData[id][bizName]);
	}
	else
	{
		if(BusinessData[id][bizDeliver][0] == 0.0 && BusinessData[id][bizDeliver][1] == 0.0 && BusinessData[id][bizDeliver][2] == 0.0)
			return SendErrorMessage(playerid, "Cua hang cua ban chua co diem van chuyen hang.");

		BusinessData[id][bizShipment] = 1;
		Business_Save(id);

		SendServerMessage(playerid, "Ban da yeu cau van chuyen hang.");
		SendJobMessage(1, COLOR_YELLOW, "** %s da yeu cau van chuyen hang toi cua hang %s, su dung [/xemhang] de chap nhan. **", ReturnName(playerid, 0), BusinessData[id][bizName]);
	}
	return 1;
}

