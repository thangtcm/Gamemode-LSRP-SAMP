CMD:chathang(playerid, params[])
{
	new
		id = Business_NearestDeliver(playerid),
		vid = GetPlayerVehicleID(playerid);

	if(PlayerData[playerid][pJob] != JOB_COURIER)
		return SendErrorMessage(playerid, "Ban khong phai la trucker.");

	if(id == -1)
		return SendErrorMessage(playerid, "Ban khong dung gan diem van chuyen hang.");

 	if(!IsLoadableVehicle(vid))
		return SendErrorMessage(playerid, "Ban phai ben trong phuong tien.");

	if(!CoreVehicles[vid][vehLoadType] || CoreVehicles[vid][vehLoads] < 1)
		return SendErrorMessage(playerid, "Phuong tien nay khong co hang.");

	if(PlayerData[playerid][pUnloading] != -1)
		return SendErrorMessage(playerid, "Ban da chat hang phuong tien nay.");

	if(PlayerData[playerid][pShipment] != -1 && id != PlayerData[playerid][pShipment])
		return SendErrorMessage(playerid, "Ban khong co chap nhan van chuyen hang cho cua hang nay.");

	switch(CoreVehicles[vid][vehLoadType])
	{
		case 1:
		{
			if(BusinessData[id][bizType] != 1 && BusinessData[id][bizType] != 6)
				return SendErrorMessage(playerid, "Cua hang nay khong co goi van chuyen hang.");
		}
		case 2..4:
		{
			if(BusinessData[id][bizType] != CoreVehicles[vid][vehLoadType])
				return SendErrorMessage(playerid, "Cua hang nay khong co goi van chuyen hang.");
		}
		case 5:
		{
			if(BusinessData[id][bizType] != 6)
				return SendErrorMessage(playerid, "Cua hang nay khong co goi van chuyen hang.");
		}
		case 6:
		{
			if(BusinessData[id][bizType] != 7)
				return SendErrorMessage(playerid, "Cua hang nay khong co goi van chuyen hang.");
		}
	}
	static
		Float:fX,
		Float:fY,
		Float:fZ;

	DisableWaypoint(playerid);

	GetVehicleBoot(vid, fX, fY, fZ);
	RemovePlayerFromVehicle(playerid);

	PlayerData[playerid][pUnloading] = id;
	PlayerData[playerid][pUnloadVehicle] = vid;

	SetPlayerCheckpoint(playerid, fX, fY, fZ, 1.0);
	SendServerMessage(playerid, "Hay chat hang va van chuyen toi dia diem.");

	return 1;
}

CMD:suaxe(playerid, params[])
{
	if(PlayerData[playerid][pJob] != JOB_MECHANIC)
		return SendErrorMessage(playerid, "Ban khong phai la sua xe.");

	if(IsPlayerInAnyVehicle(playerid))
		return SendErrorMessage(playerid, "Ban phai xuong xe truoc.");

	if(!Inventory_HasItem(playerid, "Bo dung cu"))
		return SendErrorMessage(playerid, "Ban khong co bo dung cu.");

	if(PlayerData[playerid][pRepairTime] > gettime())
		return SendErrorMessage(playerid, "Ban phai cho %d giay moi co the sua xe tiep.", PlayerData[playerid][pRepairTime] - gettime());

	for(new i = 1; i != MAX_VEHICLES; i++) if(IsValidVehicle(i) && IsPlayerNearHood(playerid, i))
	{
		if(!IsEngineVehicle(i))
			return SendErrorMessage(playerid, "Phuong tien nay khong the sua.");

		if(!GetHoodStatus(i))
			return SendErrorMessage(playerid, "Mui xe phai mo moi co the sua.");

		if(CoreVehicles[i][vehRepairing])
			return SendErrorMessage(playerid, "Phuong tien nay dang duoc sua.");

		Inventory_Remove(playerid, "Bo dung cu");
		ApplyAnimation(playerid, "BD_FIRE", "wash_up", 4.1, 0, 0, 0, 0, 0, 1);

		CoreVehicles[i][vehRepairing] = true;
		SetTimerEx("RepairCar", 5000, false, "dd", playerid, i);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s dang bat dau sua xe.", ReturnName(playerid, 0));
		GameTextForPlayer(playerid, "~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~~g~Dang sua...~w~ Xui long cho", 5500, 3);
		return 1;
	}
	SendErrorMessage(playerid, "Ban khong dung gan phuong tien.");
	return 1;
}

CMD:nitro(playerid, params[])
{
	if(PlayerData[playerid][pJob] != JOB_MECHANIC)
		return SendErrorMessage(playerid, "Ban khong phai la sua xe.");

	if(IsPlayerInAnyVehicle(playerid))
		return SendErrorMessage(playerid, "Ban phai ra khoi xe truoc.");

	if(!Inventory_HasItem(playerid, "Nitro"))
		return SendErrorMessage(playerid, "Ban khong co nitro.");

	for(new i = 1; i != MAX_VEHICLES; i++) if(IsValidVehicle(i) && IsPlayerNearHood(playerid, i))
	{
		if(!IsEngineVehicle(i) || IsABike(i) || IsABoat(i) || IsAPlane(i) || IsAHelicopter(i))
			return SendErrorMessage(playerid, "Ban khong the them nitro vao phuong tien nay.");

		if(!GetHoodStatus(i))
			return SendErrorMessage(playerid, "Mui xe phai mo moi co the them nitro.");

		Inventory_Remove(playerid, "Nitro");
		ApplyAnimation(playerid, "BD_FIRE", "wash_up", 4.1, 0, 0, 0, 0, 0, 1);

		AddComponent(i, 1010);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da dat nitro vao phuong tien.", ReturnName(playerid, 0));
		return 1;
	}
	SendErrorMessage(playerid, "Ban khong dung gan phuong tien nao.");
	return 1;
}

