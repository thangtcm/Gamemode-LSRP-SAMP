		}
		else if(!strcmp(type, "business", true))
		{
			if(sscanf(string, "d", id))
				return SendSyntaxMessage(playerid, "/goto [business] [business ID]");

			if((id < 0 || id >= MAX_BUSINESSES) || !BusinessData[id][bizExists])
