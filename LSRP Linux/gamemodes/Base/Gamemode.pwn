CMD:call(playerid, params[])
{
    if(!Inventory_HasItem(playerid, "Dien thoai"))
		return SendErrorMessage(playerid, "Ban khong co dien thoai.");

    if(PlayerData[playerid][pPhoneOff])
		return SendErrorMessage(playerid, "Dien thoai cua ban da tat nguon.");

    if(PlayerData[playerid][pHospital] != -1 || PlayerData[playerid][pCuffed] || PlayerData[playerid][pInjured] || !IsPlayerSpawned(playerid))
		return SendErrorMessage(playerid, "Ban khong the lam dieu nay ngay bay gio.");

	static
		targetid,
		number;

	if(sscanf(params, "d", number))
		return SendSyntaxMessage(playerid, "/call [phone number] 1222-Taxi, 911-Police/FD, 222-News Corp, 223-Billboards Agency, 115-Mechanic, 34245-Government/Bank.");

	if(!number)
		return SendErrorMessage(playerid, "So dien thoai khong hop le.");

	if(number == 911)
	{
		PlayerData[playerid][pEmergency] = 1;
		PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);

		SetPlayerAttachedObject(playerid, 9, 330, 6);
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay dien thoai tu trong tui do ra.", ReturnName(playerid, 0));
		SendClientMessage(playerid, COLOR_LIGHTBLUE, "911, ban muon su dung dich vu nao ? \"Police\" hay \"FD\"?");
	}
	else if(number == 1222)
	{
		PlayerData[playerid][pTaxiCalled] = 1;
		PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay dien thoai tu trong tui do ra.", ReturnName(playerid, 0));
		SendClientMessage(playerid, COLOR_YELLOW, "Taxi da xac nhan duoc cuoc goi cua ban.");

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da cat dien thoai va bo vao trong tui do.", ReturnName(playerid, 0));

		SendJobMessage(3, COLOR_YELLOW, "|__________________ Cuoc goi Taxi __________________|");
		SendJobMessage(3, COLOR_YELLOW, "Nguoi goi: %s, PH:[%d]", ReturnName(playerid, 0), PlayerData[playerid][pPhone]);
		SendJobMessage(3, COLOR_YELLOW, "Vi tri: %s", GetPlayerLocation(playerid));
		SendJobMessage(3, COLOR_YELLOW, "Su dung [/call] hoac [/acceptcall].");
	}
	else if(number == 115)
	{
		PlayerData[playerid][pMechanicCalled] = 1;
		PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay dien thoai tu trong tui do ra.", ReturnName(playerid, 0));
		SendClientMessage(playerid, COLOR_YELLOW, "Tho sua xe da xac nhan duoc cuoc goi cua ban.");

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da cat dien thoai va bo vao trong tui do.", ReturnName(playerid, 0));

		SendJobMessage(2, COLOR_DARKGREEN, "|__________________ Cuoc goi sua xe __________________|");
		SendJobMessage(2, COLOR_WHITE, "Nguoi goi: %s, PH:[%d]", ReturnName(playerid, 0), PlayerData[playerid][pPhone]);
		SendJobMessage(2, COLOR_WHITE, "Vi tri: %s", GetPlayerLocation(playerid));
		SendJobMessage(2, COLOR_WHITE, "Su dung [/call] hoac [/acceptmechanic].");
	}
	else if(number == 222)
	{
		PlayerData[playerid][pPlaceAd] = 1;
		PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);

		SetPlayerAttachedObject(playerid, 9, 330, 6);
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay dien thoai tu trong tui do ra.", ReturnName(playerid, 0));

		if(PlayerData[playerid][pPlayingHours] < 2)
		{
			SendClientMessage(playerid, COLOR_CYAN, "Xin loi, ban phai online 2 choi moi co the dat quang cao.");
			cmd_hangup(playerid, "\1");
		}
		else if(PlayerData[playerid][pAdTime] < 1)
		{
			SendClientMessage(playerid, COLOR_CYAN, "Hay noi \"yes\" neu ban muon dat quang cao $500.");
		}
		else
		{
			SendClientMessage(playerid, COLOR_CYAN, "Ban da dat quang cao 2 phut truoc, xin vui long thu lai.");
			cmd_hangup(playerid, "\1");
		}
	}
	else if(number == 223)
	{
		PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);

		SetPlayerAttachedObject(playerid, 9, 330, 6);
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay dien thoai tu trong tui do ra.", ReturnName(playerid, 0));

		if(PlayerData[playerid][pPlayingHours] < 2)
		{
			SendClientMessage(playerid, COLOR_CYAN, "Xin loi, ban phai online 2 choi moi co the thue billboard.");
			cmd_hangup(playerid, "\1");
		}
		SendClientMessageEx(playerid, COLOR_YELLOW, "[Phone]:{FFFFFF} Xin chao, day la Los Santos Billboard, hay lang nghe nhung lua chon sau.");
		ViewBillboards(playerid);
	}
	else if((targetid = GetNumberOwner(number)) != INVALID_PLAYER_ID)
	{
		if(targetid == playerid)
			return SendErrorMessage(playerid, "Ban khong the goi cho chinh minh.");

		if(PlayerData[targetid][pPhoneOff])
			return SendErrorMessage(playerid, "Dien thoai cua ban da tat nguon.");

		PlayerData[targetid][pIncomingCall] = 1;
		PlayerData[playerid][pIncomingCall] = 1;

		PlayerData[targetid][pCallLine] = playerid;
		PlayerData[playerid][pCallLine] = targetid;

		SendClientMessageEx(playerid, COLOR_YELLOW, "[Phone]:{FFFFFF} Dang goi dien thoai cho so %d...", number);
		SendClientMessageEx(targetid, COLOR_YELLOW, "[Phone]:{FFFFFF} Mot cuoc goi dang goi den %d (su dung \"/pickup\" de nhac may).", PlayerData[playerid][pPhone]);

		PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);
		PlayerPlaySoundEx(targetid, 23000);
		SetPlayerAttachedObject(playerid, 9, 330, 6);
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay dien thoai tu trong tui do ra.", ReturnName(playerid, 0));
	}
	else if(number == 58696)
	{
		if(GetFactionType(playerid) != FACTION_GANG)
		return SendErrorMessage(playerid, "Ban khong phai la gangster.");
		
		PlayerData[playerid][pDealerCall] = 1;
		PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);

		SetPlayerAttachedObject(playerid, 9, 330, 6);
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay dien thoai tu trong tui do ra.", ReturnName(playerid, 0));
		SendClientMessage(playerid, COLOR_NEWBLUE, "[Dealer]:{FFFFFF} Yo, may can gi nao ? \"Melee Parts\", \"Pistol Parts\", \"SMG Parts\", \"Shotgun Parts\", \"Rifle Parts\", \"Drugs\" ");
	}
	else if(number == 34245)
	{
		PlayerData[playerid][pLoanAsk] = 1;
		PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);

		SetPlayerAttachedObject(playerid, 9, 330, 6);
		SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da lay dien thoai tu trong tui do ra.", ReturnName(playerid, 0));
		SendClientMessage(playerid, COLOR_ORANGE, "[Ngan hang]:{FFFFFF} Xin chao, toi la tu van tro giup. Ban can gi ? \"Loan\" or \"Help\"?");
	}
	else
	{
	    SendErrorMessage(playerid, "So dien thoai khong hop le.");
	}
	return 1;
}

CMD:createbillboard(playerid, params[])
{
	static
		id = -1;

    if(PlayerData[playerid][pAdmin] < 5)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	id = Billboard_Create(playerid, -1);

	if(id == -1)
		return SendErrorMessage(playerid, "Ban khong the tao them billboards.");

	SendServerMessage(playerid, "Ban da tao billboard ID: %d.", id);
	return 1;
}

CMD:billboards(playerid, params[])
{
	if(PlayerData[playerid][pAdmin] < 1)
	{
		SendErrorMessage(playerid, "Ban khong duoc phep su dug lenh nay");
		return 1;
	}
	ViewBillboards(playerid);
	return 1;
}

CMD:mybillboard(playerid, params[])
{
	if(PlayerData[playerid][pOwnsBillboard] == -1)
	{
		SendErrorMessage(playerid, "Ban khong so huu billboard.");
		return 1;
	}
	Dialog_Show(playerid, MyBillboardMenu, DIALOG_STYLE_LIST, "Quan ly Billboard", "Chinh sua tin nhan\nHuy thue Billboard", "Lua chon", "Huy bo");
	return 1;
}
