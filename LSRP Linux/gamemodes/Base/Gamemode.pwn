CMD:sethelper(playerid, params[])
{
	static
		userid,
		level;

	if(PlayerData[playerid][pAdmin] < 4 && PlayerData[playerid][pHelper] < 5)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(sscanf(params, "ud", userid, level))
		return SendSyntaxMessage(playerid, "/sethelper [playerid/name] [level]");

	if(userid == INVALID_PLAYER_ID)
		return SendErrorMessage(playerid, "Nguoi choi khong hop le.");

	if(level < 0 || level > 6)
		return SendErrorMessage(playerid, "Helper level khong hop le phai tu 0 den 6.");

	if(PlayerData[userid][pAdmin] > 1)
		return SendErrorMessage(playerid, "Ban khong the cho Administrator lam Helper.");

	if(level > PlayerData[userid][pHelper])
	{
		SendHelperAction(playerid, "Ban da tuyen dung %s len level [%d].", ReturnName(userid, 0), level);
		SendHelperAction(userid, "%s da tuyen dung ban len level [%d].", ReturnName(playerid, 0), level);
	}
	else
	{
		SendHelperAction(playerid, "Ban da ha chuc %s xuong level [%d].", ReturnName(userid, 0), level);
		SendHelperAction(userid, "%s da ha chuc ban xuong level [%d].", ReturnName(playerid, 0), level);
	}
	PlayerData[userid][pHelper] = level;
	Log_Write("logs/helper_log.txt", "[%s] %s da set %s len level [%d].", ReturnDate(), ReturnName(playerid, 0), ReturnName(userid, 0), level);
	return 1;
}

