	static
		Float:amount;

	GetPlayerHealth(playerid, amount);
	return floatround(amount, floatround_round);
}

ReturnName(playerid, underscore=1)
{
	static
		name[MAX_PLAYER_NAME + 1];

	GetPlayerName(playerid, name, sizeof(name));

	if(!underscore)
	{
		for(new i = 0, len = strlen(name); i < len; i++)
		{
			if(name[i] == '_') name[i] = ' ';
		}
	}
	if(PlayerData[playerid][pMaskOn])
		format(name, sizeof(name), "[Mask %d_%d]", PlayerData[playerid][pMaskID][0], PlayerData[playerid][pMaskID][1]);

	return name;
}

ReturnIP(playerid)
{
	static
		ip[16];

	GetPlayerIp(playerid, ip, sizeof(ip));
	return ip;
}

Payday(i)
{
	static
		str[128];

	new paycheck = random(100) + 100;
	new factionpay = random(1000) + 3000;

	new
		Float: interest,
		interest_convert;

	interest = (PlayerData[i][pBankMoney] / 100) * 0.1; 
	interest_convert = floatround(interest, floatround_round); 

	PlayerData[i][pMinutes] = 0;

	PlayerData[i][pExp]++;

	PlayerData[i][pPlayingHours]++;

	PlayerData[i][pPaydayID] = -1;

	PlayerTextDrawHide(i, PlayerData[i][pTextdraws][83]);

	if(PlayerData[i][pOwnsBillboard] >= 0)
	{
		if(PlayerData[i][pBankMoney] >= BillBoardData[PlayerData[i][pOwnsBillboard]][bbPrice])
		{
			SendClientMessage(i, COLOR_GREY, "-----------------------------------------------------------");
			SendClientMessageEx(i, COLOR_WHITE, "Tien lanh luong {33CC33}%s{FFFFFF} da duoc them vao tai khoan ngan hang.", FormatNumber(paycheck));
			SendClientMessageEx(i, COLOR_WHITE, "{33CC33}%s{FFFFFF} da khau tru phi tai khoan ngan hang tu billboard.", FormatNumber(BillBoardData[PlayerData[i][pOwnsBillboard]][bbPrice]));
			SendClientMessage(i, COLOR_GREY, "-----------------------------------------------------------");
			PlayerData[i][pBankMoney] -= BillBoardData[PlayerData[i][pOwnsBillboard]][bbPrice];
			Tax_AddMoney(BillBoardData[PlayerData[i][pOwnsBillboard]][bbPrice]);
			return 1;
		}
		if(PlayerData[i][pBankMoney] < BillBoardData[PlayerData[i][pOwnsBillboard]][bbPrice])
		{
			SendClientMessage(i, COLOR_GREY, "-----------------------------------------------------------");
			SendClientMessageEx(i, COLOR_WHITE, "Tien lanh luong {33CC33}%s{FFFFFF} da duoc them vao tai khoan ngan hang.", FormatNumber(paycheck));
			SendClientMessageEx(i, COLOR_LIGHTRED, "Ban khong the tra tien billboard cua ban nua, do do no da duoc thue.");
			SendClientMessage(i, COLOR_GREY, "-----------------------------------------------------------");
			BillBoardData[PlayerData[i][pOwnsBillboard]][bbOwner] = 0;
			Billboard_Save(PlayerData[i][pOwnsBillboard]);
			Billboard_Refresh(PlayerData[i][pOwnsBillboard]);
			PlayerData[i][pOwnsBillboard] = -1;
		}
		return 1;
	}

	SendClientMessage(i, COLOR_GREY, "|____ BANK STATEMENT ____|");
	SendClientMessageEx(i, COLOR_WHITE, "		Tai khoan cu: %s", FormatNumber(PlayerData[i][pBankMoney]));
	SendClientMessage(i, COLOR_WHITE, "		Lai suat: 0.1");
	SendClientMessageEx(i, COLOR_WHITE, "		Lai suat dat duoc: %s", FormatNumber(interest_convert));
	SendClientMessageEx(i, COLOR_WHITE, "		Paycheck: %s", FormatNumber(paycheck));

	if(GetFactionType(i) == FACTION_POLICE || GetFactionType(i) == FACTION_NEWS || GetFactionType(i) == FACTION_MEDIC || GetFactionType(i) == FACTION_GOV || GetFactionType(i) == FACTION_COMPANY)
		SendClientMessageEx(i, COLOR_WHITE, "		Faction: %s", FormatNumber(factionpay));

	if(GetFactionType(i) == FACTION_POLICE || GetFactionType(i) == FACTION_NEWS || GetFactionType(i) == FACTION_MEDIC || GetFactionType(i) == FACTION_GOV || GetFactionType(i) == FACTION_COMPANY)
	{
		PlayerData[i][pBankMoney] += paycheck;
		PlayerData[i][pBankMoney] += factionpay;
	}
	else
	{
		PlayerData[i][pBankMoney] += paycheck;
	}

	SendClientMessage(i, COLOR_GREY, "|_________________________|");

	SendClientMessageEx(i, COLOR_WHITE, "		Tai khoan moi: %s", FormatNumber(PlayerData[i][pBankMoney]));

	if(PlayerData[i][pJob] == JOB_MECHANIC)
		SendClientMessage(i, COLOR_WHITE, "(( Ban duoc them $1,250 vao tai khoan cua ban. ))");

	new exp_count = ((PlayerData[i][pLevel]) * 4 + 2);
	if(PlayerData[i][pLevel] == 1 && PlayerData[i][pExp] >= exp_count || PlayerData[i][pLevel] == 2 && PlayerData[i][pExp] >= exp_count || PlayerData[i][pDonator] > 2 && PlayerData[i][pExp] >= exp_count)
	{
		PlayerData[i][pLevel]++;
		PlayerData[i][pExp] = 0;

		PlayerPlaySound(i, 1052, 0.0, 0.0, 0.0);
		SetPlayerScore(i, PlayerData[i][pLevel]);

		format(str, sizeof(str), "~g~Level up~n~~w~Ban da duoc len level %d.", PlayerData[i][pLevel]);
		GameTextForPlayer(i, str, 5000, 1);
	}

	if(GetFactionType(i) == FACTION_POLICE || GetFactionType(i) == FACTION_NEWS || GetFactionType(i) == FACTION_MEDIC || GetFactionType(i) == FACTION_GOV || GetFactionType(i) == FACTION_COMPANY)
	{
		format(str, sizeof(str), "~y~Payday~n~~w~FactionPay~n~~g~%s", FormatNumber(factionpay));
		GameTextForPlayer(i, str, 3000, 1);
	}
	else
	{
		format(str, sizeof(str), "~y~Payday~n~~w~Paycheck~n~~g~%s", FormatNumber(paycheck));
		GameTextForPlayer(i, str, 3000, 1);
	}
	return 1;
}

