{
	if(rackid != -1 && RackData[rackid][rackExists])
	{
		static
			str[64];

		if(IsValidDynamicObject(RackData[rackid][rackObjects][4]))
		{
			DestroyDynamicObject(RackData[rackid][rackObjects][4]);
		}
