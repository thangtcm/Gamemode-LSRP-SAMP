	if(!Inventory_HasItem(playerid, "But"))
		return SendErrorMessage(playerid, "Ban khong co viet de ghi giay, hay mua no tai cua hang buu dien.");

	if(sscanf(params, "uds[64]", userid, amount, reason))
		return SendSyntaxMessage(playerid, "/offerloan [playerid/name] [price] [reason]");

	if(amount < 500 || amount > 500000)
		return SendErrorMessage(playerid, "So luong tien khong the duoi $500 hoac cao hon $500,000.");

	if(userid == INVALID_PLAYER_ID)
