	}
	else SendErrorMessage(playerid, "Ban khong dung gan dung gan nha cua hoac phuong tien nao de khoa.");
	return 1;
}

CMD:sell(playerid, params[])
{
	static
		targetid,
		type[24],
		string[128];

	if(sscanf(params, "us[24]S()[128]", targetid, type, string))
	{
		SendSyntaxMessage(playerid, "/sell [playerid/name] [name]");
		SendClientMessage(playerid, COLOR_YELLOW, "Lua chon: house, business, vehicle.");
		return 1;
	}
	if(targetid == INVALID_PLAYER_ID || !IsPlayerNearPlayer(playerid, targetid, 5.0))
	{
		SendErrorMessage(playerid, "Nguoi choi do khong gan ban.");
		return 1;
	}
	if(targetid == playerid)
	{
		SendErrorMessage(playerid, "Ban khong the ban cho chinh minh.");
		return 1;
	}
	if(!strcmp(type, "house", true))
	{
		static
			price,
			houseid = -1;

		if(sscanf(string, "d", price))
			return SendSyntaxMessage(playerid, "/sell [playerid/name] [house] [price]");

		if(price < 1)
			return SendErrorMessage(playerid, "Gia tien khong the duoi $1.");

		if((houseid = House_Nearest(playerid)) != -1 && House_IsOwner(playerid, houseid))
		{
			PlayerData[targetid][pHouseSeller] = playerid;
			PlayerData[targetid][pHouseOffered] = houseid;
			PlayerData[targetid][pHouseValue] = price;

			SendServerMessage(playerid, "Ban da ban can nha cho %s voi gia %s.", ReturnName(targetid, 0), FormatNumber(price));
			SendServerMessage(targetid, "%s da ban can nha cho ban voi gia %s (Su dung: \"[/chapnhan house]\").", ReturnName(playerid, 0), FormatNumber(price));
		}
		else SendErrorMessage(playerid, "Ban khong dung gan can nha nao.");
	}
	else if(!strcmp(type, "business", true))
	{
		static
			price,
			bizid = -1;

		if(sscanf(string, "d", price))
			return SendSyntaxMessage(playerid, "/sell [playerid/name] [business] [price]");

		if(price < 1)
		    return SendErrorMessage(playerid, "Gia tien khong the duoi $1.");

		if((bizid = Business_Nearest(playerid)) != -1 && Business_IsOwner(playerid, bizid))
		{
			PlayerData[targetid][pBusinessSeller] = playerid;
			PlayerData[targetid][pBusinessOffered] = bizid;
			PlayerData[targetid][pBusinessValue] = price;

			SendServerMessage(playerid, "Ban da ban cua hang cho %s voi gia %s.", ReturnName(targetid, 0), FormatNumber(price));
			SendServerMessage(targetid, "%s da ban cua hang cho ban voi gia %s (Su dung \"[/chapnhan business]\").", ReturnName(playerid, 0), FormatNumber(price));
		}
		else SendErrorMessage(playerid, "Ban khong dung gan cua hang.");
	}
	else if(!strcmp(type, "vehicle", true))
	{
		static
			price,
			carid = -1;

		if(sscanf(string, "d", price))
			return SendSyntaxMessage(playerid, "/sell [playerid/name] [veh] [price]");

		if(price < 1)
			return SendErrorMessage(playerid, "Gia tien khong the duoi $1.");

		if((carid = Car_Inside(playerid)) != -1 && Car_IsOwner(playerid, carid))
		{
			PlayerData[targetid][pCarSeller] = playerid;
			PlayerData[targetid][pCarOffered] = carid;
			PlayerData[targetid][pCarValue] = price;

			SendServerMessage(playerid, "Ban da ban phuong tien %s cho %s voi gia %s.", ReturnVehicleModelName(CarData[carid][carModel]), ReturnName(targetid, 0), FormatNumber(price));
			SendServerMessage(targetid, "%s da ban phuong tien %s cho ban voi gia %s (type \"/approve car\").", ReturnVehicleModelName(CarData[carid][carModel]), ReturnName(playerid, 0), FormatNumber(price));
		}
		else SendErrorMessage(playerid, "Ban khong dung gan phuong tien so huu cua ban.");
	}
	return 1;
}

