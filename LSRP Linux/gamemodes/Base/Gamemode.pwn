CMD:listcars(playerid, params[])
{
	new
		Float:fX,
		Float:fY,
		Float:fZ,
		userid,
		count;

	if(sscanf(params, "u", userid))
	{
		SendClientMessage(playerid, COLOR_GREY, "-----------------------------------------------------------");

		for(new i = 0; i < MAX_DYNAMIC_CARS; i++) if(Car_IsOwner(playerid, i))
		{
			GetVehiclePos(CarData[i][carVehicle], fX, fY, fZ);

			SendClientMessageEx(playerid, COLOR_WHITE, "** ID: %d | Model: %s | Vi tri: %s", CarData[i][carVehicle], ReturnVehicleModelName(CarData[i][carModel]), GetLocation(fX, fY, fZ));
			count++;
		}
		if(!count)
			SendClientMessage(playerid, COLOR_WHITE, "Ban khong co so huu phuong tien nao.");

		SendClientMessage(playerid, COLOR_GREY, "-----------------------------------------------------------");
	}
	else if(PlayerData[playerid][pAdmin] >= 3)
	{
		if(userid == INVALID_PLAYER_ID)
			return SendErrorMessage(playerid, "Nguoi choi khong hop le.");

