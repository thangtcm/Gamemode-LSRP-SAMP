CMD:setfuel(playerid, params[])
{
	static
		id = 0,
		amount;

	if(PlayerData[playerid][pAdmin] < 5)
		return SendErrorMessage(playerid, "Ban khong duoc phep su dung lenh nay.");

	if(sscanf(params, "dd", id, amount))
	{
		if(IsPlayerInAnyVehicle(playerid))
		{
			id = GetPlayerVehicleID(playerid);

			if(sscanf(params, "d", amount))
				return SendSyntaxMessage(playerid, "/setfuel [amount]");

			if(amount < 0)
				return SendErrorMessage(playerid, "So luong xang khong the duoi 0.");

			CoreVehicles[id][vehFuel] = amount;
			SendServerMessage(playerid, "Ban da cho xang phuong tien ID: %d voi so luong %d lit.", id, amount);
			return 1;
		}
		else return SendSyntaxMessage(playerid, "/setfuel [vehicle id] [amount]");
	}
	if(!IsValidVehicle(id))
		return SendErrorMessage(playerid, "ID phuong tien khong hop le.");

	if(amount < 0)
		return SendErrorMessage(playerid, "So luong xang khong the duoi 0.");

	CoreVehicles[id][vehFuel] = amount;
	SendServerMessage(playerid, "Ban da cho xang phuong tien ID: %d voi so luong %d lit.", id, amount);
	return 1;
}

