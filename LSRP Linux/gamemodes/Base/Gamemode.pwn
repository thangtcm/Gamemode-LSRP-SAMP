forward BreakCuffs(playerid, userid);
public BreakCuffs(playerid, userid)
{
	if(PlayerData[playerid][pCuffed] || PlayerData[playerid][pInjured] || !IsPlayerSpawned(playerid) || !Inventory_HasItem(playerid, "Xa beng") || !IsPlayerNearPlayer(playerid, userid, 6.0) || !PlayerData[userid][pCuffed])
		return 1;

	if(random(2))
	{
		ShowPlayerFooter(playerid, "Ban da ~r~that bai~w~ cho viec be cong.");
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da be cong that bai.", ReturnName(playerid, 0));
	}
	else
	{
		PlayerData[userid][pCuffed] = 0;
		SetPlayerSpecialAction(userid, SPECIAL_ACTION_NONE);

		ShowPlayerFooter(playerid, "Ban da ~g~thanh cong~w~ cho viec be cong.");
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s da be cong thanh cong %s.", ReturnName(playerid, 0), ReturnName(userid, 0));
	}
	return 1;
}

