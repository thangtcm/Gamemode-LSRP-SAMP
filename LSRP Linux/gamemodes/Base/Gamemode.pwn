Dialog:Inventory(playerid, response, listitem, inputtext[])
{
	if(response)
	{
		new
			itemid = PlayerData[playerid][pInventoryItem],
			string[64];

		strunpack(string, InventoryData[playerid][itemid][invItem]);

		switch(listitem)
		{
			case 0:
			{
				if(!strcmp(string, "Soda") && PlayerData[playerid][pTutorialStage] == 3)
				{
					PlayerData[playerid][pThirst] = 100;
					Dialog_Show(playerid, Tutorial, DIALOG_STYLE_MSGBOX, "Huong dan", "Ban da hoc lam sao de nhat item va su dung no\nBan da nhat soda va co the su dung no khi khat nuoc.\n\nTrang thai doi bung va khat nuoc se hien thi ben phai man hinh.\nNeu trang thai va khat nuoc ve so 0, ban co the bi chet vi doi bung hoac khat nuoc.", "Tiep theo", "");
				}
				else
				{
					CallLocalFunction("OnPlayerUseItem", "dds", playerid, itemid, string);
				}
			}
			case 1:
			{
				if(!strcmp(string, "Soda"))
				    return 0;

				PlayerData[playerid][pInventoryItem] = itemid;
				Dialog_Show(playerid, GiveItem, DIALOG_STYLE_INPUT, "Cho item", "Xin vui long nhap ten nguoi choi hoac ID:", "Xac nhan", "Huy bo");
			}
			case 2:
			{
				new id = -1;

				if(!strcmp(string, "Soda") && PlayerData[playerid][pTutorialStage] != 4)
				    return 0;

				if(PlayerData[playerid][pTutorialStage] == 4)
				{
					Inventory_Remove(playerid, "Soda");
					Dialog_Show(playerid, Tutorial, DIALOG_STYLE_MSGBOX, "Huong dan", "Ban da vut soda. Ban co the nhat item su dung nut 'N'.\nBan co the cho item hoac trao doi item tu trong tui do cua ban sang item cho nguoi choi.\nBan co the cat item vao can nha hoac phuong tien cua ban.\nTui do nhan vat cua ban co the chua duoc %d item.", "Tiep tuc", "", MAX_INVENTORY);
					return 1;
				}
				if(IsPlayerInAnyVehicle(playerid) || !IsPlayerSpawned(playerid))
					return SendErrorMessage(playerid, "Ban khong the vut item ngay bay gio.");

				else if(!strcmp(string, "Backpack"))
					return cmd_dropbackpack(playerid, "\1");

				else if((id = Garbage_Nearest(playerid)) != -1)
				{
					if(GarbageData[id][garbageCapacity] >= 20)
						return SendErrorMessage(playerid, "Thung rac nay da chua rat nhieu rac.");

					GarbageData[id][garbageCapacity]++;
					Garbage_Save(id);

					Inventory_Remove(playerid, string);
					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "* %s throws a \"%s\" into the garbage bin.", ReturnName(playerid, 0), string);

					format(string, sizeof(string), "[Thung rac %d]\n{FFFFFF}So luong rac: %d/20", id, GarbageData[id][garbageCapacity]);
					UpdateDynamic3DTextLabelText(GarbageData[id][garbageText3D], COLOR_YELLOW, string);
				}
				else if(InventoryData[playerid][itemid][invQuantity] == 1)
					DropPlayerItem(playerid, itemid);

				else
					Dialog_Show(playerid, DropItem, DIALOG_STYLE_INPUT, "Vut item", "Item: %s - So luong: %d\n\nXin vui long nhap so luong ban muon vut item nay:", "Vut", "Huy bo", string, InventoryData[playerid][itemid][invQuantity]);
			}
		}
	}
	return 1;
}

Dialog:DropItem(playerid, response, listitem, inputtext[])
{
	new
		itemid = PlayerData[playerid][pInventoryItem],
		string[32];

	strunpack(string, InventoryData[playerid][itemid][invItem]);

	if(response)
	{
		if(isnull(inputtext))
			return Dialog_Show(playerid, DropItem, DIALOG_STYLE_INPUT, "Vut item", "Item: %s - So luong: %d\n\nXin vui long nhap so luong ban muon vut item nay:", "Vut", "Huy bo", string, InventoryData[playerid][itemid][invQuantity]);

		if(strval(inputtext) < 1 || strval(inputtext) > InventoryData[playerid][itemid][invQuantity])
			return Dialog_Show(playerid, DropItem, DIALOG_STYLE_INPUT, "Vut item", "So luong khong hop le.\n\nItem: %s - So luong: %d\n\nXin vui long nhap so luong ban muon vut item nay:", "Vut", "Huy bo", string, InventoryData[playerid][itemid][invQuantity]);

		DropPlayerItem(playerid, itemid, strval(inputtext));
	}
	return 1;
}

